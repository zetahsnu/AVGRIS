{% for item in data if total > 0 %}
    <tr><td title="{{ item.DISSTA }}" {% if item.DISSTA == 'No Longer Available' %} style="color:red" {% endif %}>{{ item.DISSTA }}</td><td title="{{ item.VINO }}">{{ item.VINO }}</td><td title="{{ item.ACCNO }}">{{ item.ACCNO }}</td><td title="{{ item.TEMPNO }}">{{ item.TEMPNO }}</td><td title="{{ item.GENUS }}">{{ item.GENUS }}</td><td title="{{ item.SPECIES }}">{{ item.SPECIES }}</td><td title="{{ item.SUBTAXA }}">{{ item.SUBTAXA }}</td><td title="{{ item.PEDCUL }}">{{ item.PEDCUL }}</td><td title="{{ item.COUNTRY }}">{{ item.COUNTRY }}</td><td><a href="{{ BASE_PATH }}/search/view/passport/{{ item.VINO }}" title="view">VIEW</a></td></tr>
        {% else %}
    <tr><td colspan="10">No records returned</td></tr>
{% endfor %}