{% embed "base.html.twig" %}
    {% block javascript %}
        {{ parent() }}
        <script src="{{ BASE_PATH }}/public/js/search/view/back.js"></script>
    {% endblock %}
    {% block css %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ BASE_PATH }}/public/css/view/default.css">
    {% endblock %}
    {% block content %}
        {% embed "/view/part/title.html.twig" %}
        {% endembed %}
        {% embed "/view/part/header.html.twig" %}
        {% endembed %}
        <section class="ac-container">
            <div>
                <input id="ac-1" name="accordion-1" type="checkbox" />
                <label for="ac-1">SEEDLING DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Germination period<br>(no. of days from sowing to first germination)</div>
                            <div class="css_td">{{ data.GERMPD }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Hypocotyl color</div>
                            <div class="css_td">{{ data.HYPCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Cotyledon color</div>
                            <div class="css_td">{{ data.COTCOL }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-2" name="accordion-1" type="checkbox" />
                <label for="ac-2">VEGETATIVE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Plant growth habit</div>
                            <div class="css_td">{{ data.PLGHBT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Plant height (cm)</div>
                            <div class="css_td">{{ data.PLHT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of internodes</div>
                            <div class="css_td">{{ data.NOINTNOD }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Branching habit</div>
                            <div class="css_td">{{ data.BRHBT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stem diameter measured at the base (mm)</div>
                            <div class="css_td">{{ data.STDIA }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stem pubescence</div>
                            <div class="css_td">{{ data.STPUB }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stem color</div>
                            <div class="css_td">{{ data.STCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf shape</div>
                            <div class="css_td">{{ data.LFSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf color</div>
                            <div class="css_td">{{ data.LFCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf pubescence</div>
                            <div class="css_td">{{ data.LFPUB }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Petiole color</div>
                            <div class="css_td">{{ data.PETCOL }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-3" name="accordion-1" type="checkbox" />
                <label for="ac-3">INFLORESCENCE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Flowering date</div>
                            <div class="css_td">{{ data.FLODATE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of nodes to first flower</div>
                            <div class="css_td">{{ data.NONODFF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Corolla color</div>
                            <div class="css_td">{{ data.CORCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Red color on the base of corolla</div>
                            <div class="css_td">{{ data.RCOLBCOR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of calyx segments</div>
                            <div class="css_td">{{ data.NOCALSEG }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Shape of calyx segments</div>
                            <div class="css_td">{{ data.SHPCALSEG }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Persistence of calyx segments</div>
                            <div class="css_td">{{ data.PERCALSEG }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-4" name="accordion-1" type="checkbox" />
                <label for="ac-4">FRUIT DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Position of fruits at the primary stem</div>
                            <div class="css_td">{{ data.POFRPSTM }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Date to fruit maturity</div>
                            <div class="css_td">{{ data.DATEFRMAT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit color</div>
                            <div class="css_td">{{ data.FRLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit length (cm)</div>
                            <div class="css_td">{{ data.FRLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit pubescence</div>
                            <div class="css_td">{{ data.FRPUB }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of locules per fruit</div>
                            <div class="css_td">{{ data.NOLOCPFR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit type</div>
                            <div class="css_td">{{ data.FRTYPE }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-5" name="accordion-1" type="checkbox" />
                <label for="ac-5">SEED DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Number of seeds per fruit</div>
                            <div class="css_td">{{ data.NOSDSPFR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed shape</div>
                            <div class="css_td">{{ data.SDSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed surface</div>
                            <div class="css_td">{{ data.SDSURF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">1000 seeds weight (gm)</div>
                            <div class="css_td">{{ data.SDWT }}</div>
                        </div>
                    </div>
                </article>
            </div>
        </section>
    {% endblock %}
{% endembed %}