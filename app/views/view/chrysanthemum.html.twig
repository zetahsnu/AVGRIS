{% embed "base.html.twig" %}
    {% block javascript %}
        {{ parent() }}
        <script src="{{ BASE_PATH }}/public/js/search/view/back.js"></script>
    {% endblock %}
    {% block css %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ BASE_PATH }}/public/css/view/default.css">
    {% endblock %}
    {% block content %}
        {% embed "/view/part/title.html.twig" %}
        {% endembed %}
        {% embed "/view/part/header.html.twig" %}
        {% endembed %}
        <section class="ac-container">
            <div>
                <input id="ac-1" name="accordion-1" type="checkbox" />
                <label for="ac-1">SEEDLING DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Anthocyanin coloration of hypocotyl<br>(when the seedling primary leaves are fully opened and the terminal bud is around 5 mm)</div>
                            <div class="css_td">{{ data.ANTHO }}</div>
                        </div>                        
                        <div class="css_tr">
                            <div class="css_td">Hypocotyl color intensity</div>
                            <div class="css_td">{{ data.HCOLIN }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-2" name="accordion-1" type="checkbox" />
                <label for="ac-2">VEGETATIVE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Plant height (cm) (N=10)</div>
                            <div class="css_td">{{ data.PLHT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Plant type</div>
                            <div class="css_td">{{ data.PLTYPE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stem color</div>
                            <div class="css_td">{{ data.STCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Branching habit</div>
                            <div class="css_td">{{ data.BHBT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf blade lobing</div>
                            <div class="css_td">{{ data.LFBLOB }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf color</div>
                            <div class="css_td">{{ data.LEFCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf shape of margin</div>
                            <div class="css_td">{{ data.LEFMAR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf surface</div>
                            <div class="css_td">{{ data.LEFSUR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf length (cm) (N=10)</div>
                            <div class="css_td">{{ data.LEFLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf width (cm) (N=10)</div>
                            <div class="css_td">{{ data.LEFWID }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-3" name="accordion-1" type="checkbox" />
                <label for="ac-3">INFLORESCENCE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Days to flower</div>
                            <div class="css_td">{{ data.DAYSFLO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Flower size (mm) (N=10)</div>
                            <div class="css_td">{{ data.FLOSZE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Petal color</div>
                            <div class="css_td">{{ data.PETCOL }}</div>
                        </div>
                    </div>
                </article>
            </div>
        </section>
    {% endblock %}
{% endembed %}