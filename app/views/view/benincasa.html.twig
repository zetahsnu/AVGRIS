{% embed "base.html.twig" %}
    {% block javascript %}
        {{ parent() }}
        <script src="{{ BASE_PATH }}/public/js/search/view/back.js"></script>
    {% endblock %}
    {% block css %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ BASE_PATH }}/public/css/view/default.css">
    {% endblock %}
    {% block content %}
        {% embed "/view/part/title.html.twig" %}
        {% endembed %}
        {% embed "/view/part/header.html.twig" %}
        {% endembed %}
        <section class="ac-container">
            <div>
                <input id="ac-1" name="accordion-1" type="checkbox" />
                <label for="ac-1">SEEDLING DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Germination period<br>(no. of days from sowing to 50% emergence)</div>
                            <div class="css_td">{{ data.GERMPD }}</div>
                        </div>                        
                        <div class="css_tr">
                            <div class="css_td">Color of cotyledon</div>
                            <div class="css_td">{{ data.COTCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of cotyledon (mm)</div>
                            <div class="css_td">{{ data.LENCOT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Width of cotyledon (mm)</div>
                            <div class="css_td">{{ data.WIDCOT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Cotyledon size</div>
                            <div class="css_td">{{ data.COTSZE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Cotyledon shape</div>
                            <div class="css_td">{{ data.COTSHP }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-2" name="accordion-1" type="checkbox" />
                <label for="ac-2">VEGETATIVE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Plant growth habit</div>
                            <div class="css_td">{{ data.PLGRWHBT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of side shoots</div>
                            <div class="css_td">{{ data.NOSIDSHT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stem shape</div>
                            <div class="css_td">{{ data.STSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Main vine length (cm)</div>
                            <div class="css_td">{{ data.MVINLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Main vine diameter (cm)</div>
                            <div class="css_td">{{ data.MVINDIA }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of inter-node (cm)</div>
                            <div class="css_td">{{ data.LENINT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Tendrils</div>
                            <div class="css_td">{{ data.TENDRIL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf color</div>
                            <div class="css_td">{{ data.LFCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf pubescence density</div>
                            <div class="css_td">{{ data.LFPUBDEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf shape</div>
                            <div class="css_td">{{ data.LFSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf lobes</div>
                            <div class="css_td">{{ data.LFLOB }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf margin</div>
                            <div class="css_td">{{ data.LFMGN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of petiole (cm)</div>
                            <div class="css_td">{{ data.LENPET }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf length (cm)</div>
                            <div class="css_td">{{ data.LFLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf width (cm)</div>
                            <div class="css_td">{{ data.LFWID }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-3" name="accordion-1" type="checkbox" />
                <label for="ac-3">INFLORESCENCE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Days to first female flower</div>
                            <div class="css_td">{{ data.DAYSFFF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Node number of first female flower</div>
                            <div class="css_td">{{ data.NODEFFF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Time of flowering</div>
                            <div class="css_td">{{ data.TIMEFLO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pistillate petal length (cm)</div>
                            <div class="css_td">{{ data.PIPETLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pistillate petal width (cm)</div>
                            <div class="css_td">{{ data.PIPETWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Sex type</div>
                            <div class="css_td">{{ data.SEXTYPE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary shape</div>
                            <div class="css_td">{{ data.OVSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary color</div>
                            <div class="css_td">{{ data.OVCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary pubescence</div>
                            <div class="css_td">{{ data.OVPUB }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary length (cm)</div>
                            <div class="css_td">{{ data.OVLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary diameter (cm)</div>
                            <div class="css_td">{{ data.OVDIA }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-4" name="accordion-1" type="checkbox" />
                <label for="ac-4">FRUIT DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Fruit set</div>
                            <div class="css_td">{{ data.FRSET }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Days to harvesting at commercial stage</div>
                            <div class="css_td">{{ data.DAYSHARV }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit shape</div>
                            <div class="css_td">{{ data.FRSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stem-end shape</div>
                            <div class="css_td">{{ data.STESHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Blossom-end fruit shape</div>
                            <div class="css_td">{{ data.BLEFRSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit skin waxiness</div>
                            <div class="css_td">{{ data.FRSKNWAX }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit skin color</div>
                            <div class="css_td">{{ data.FRSKNCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit skin texture</div>
                            <div class="css_td">{{ data.FRSKNTEX }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit mottling</div>
                            <div class="css_td">{{ data.FRMOT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit pubescence</div>
                            <div class="css_td">{{ data.FRPUB }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Peduncle transactional shape</div>
                            <div class="css_td">{{ data.PEDTRSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of peduncle (cm)</div>
                            <div class="css_td">{{ data.LENPED }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Peduncle thickness (mm)</div>
                            <div class="css_td">{{ data.PEDTHIC }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit weight (g)</div>
                            <div class="css_td">{{ data.FRWT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit length at table use maturity (cm)</div>
                            <div class="css_td">{{ data.FRLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit width at table use maturity (cm)</div>
                            <div class="css_td">{{ data.FRWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Flesh thickness (mm)</div>
                            <div class="css_td">{{ data.FLTHIC }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Flesh texture</div>
                            <div class="css_td">{{ data.FLTEX }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed cavity</div>
                            <div class="css_td">{{ data.SDCAV }}</div>
                        </div>
                    </div>
                </article>
            </div>
             <div>
                <input id="ac-5" name="accordion-1" type="checkbox" />
                <label for="ac-5">SEED DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Number of seeds per fruit</div>
                            <div class="css_td">{{ data.NOSDSPF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ease of seed separation from placenta</div>
                            <div class="css_td">{{ data.EASDSEP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed surface luster</div>
                            <div class="css_td">{{ data.SDSURLUS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed color</div>
                            <div class="css_td">{{ data.SDCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">1000 seeds weight (gm)</div>
                            <div class="css_td">{{ data.SDWT }}</div>
                        </div>
                    </div>
                </article>
            </div>
