{% embed "base.html.twig" %}
    {% block javascript %}
        {{ parent() }}
        <script src="{{ BASE_PATH }}/public/js/search/view/back.js"></script>
    {% endblock %}
    {% block css %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ BASE_PATH }}/public/css/view/default.css">
    {% endblock %}
    {% block content %}
        {% embed "/view/part/title.html.twig" %}
        {% endembed %}
        {% embed "/view/part/header.html.twig" %}
        {% endembed %}
        <section class="ac-container">
            <div>
                <input id="ac-1" name="accordion-1" type="checkbox" />
                <label for="ac-1">SEEDLING DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Germination period<br>(No. of days from sowing to first germination)</div>
                            <div class="css_td">{{ data.GERMPD }}</div>
                        </div>                        
                        <div class="css_tr">
                            <div class="css_td">Cotyledon Color</div>
                            <div class="css_td">{{ data.COTCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Hypocotyl color</div>
                            <div class="css_td">{{ data.HYPCOL }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-2" name="accordion-1" type="checkbox" />
                <label for="ac-2">VEGETATIVE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Growth habit</div>
                            <div class="css_td">{{ data.GRHBT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Plant height (cm)<br>(at flowering stage)</div>
                            <div class="css_td">{{ data.PLHT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Tripinnatum leaf length (cm)</div>
                            <div class="css_td">{{ data.TRILFLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Tripinnatum leaf width (cm)</div>
                            <div class="css_td">{{ data.TRILFWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf type</div>
                            <div class="css_td">{{ data.LFTYP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf arrangement</div>
                            <div class="css_td">{{ data.LFARR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaflet shape</div>
                            <div class="css_td">{{ data.LFLSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaflet arrangement</div>
                            <div class="css_td">{{ data.LFLARR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of secondary rachis</div>
                            <div class="css_td">{{ data.NOSECRA }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf Color</div>
                            <div class="css_td">{{ data.LFCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf flavor</div>
                            <div class="css_td">{{ data.LFFLA }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Tripinnatum leaflet length (cm)</div>
                            <div class="css_td">{{ data.TRLFLLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Tripinnatum leaflet width (cm)</div>
                            <div class="css_td">{{ data.TRLFLWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Petiole pigmentation</div>
                            <div class="css_td">{{ data.PETPIG }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-3" name="accordion-1" type="checkbox" />
                <label for="ac-3">INFLORESCENCE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Days to flowering<br>(from sowing to 50% with inflorescence)</div>
                            <div class="css_td">{{ data.DAYSFLO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Flower Color</div>
                            <div class="css_td">{{ data.FLCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Flower aromatic</div>
                            <div class="css_td">{{ data.FLAROM }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">No. of Petals</div>
                            <div class="css_td">{{ data.NOPTL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Petal length (cm)</div>
                            <div class="css_td">{{ data.PTLLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Petal width (cm)</div>
                            <div class="css_td">{{ data.PTLWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Sepal length (cm)</div>
                            <div class="css_td">{{ data.SEPLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Sepal width (cm)</div>
                            <div class="css_td">{{ data.SEPWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">No. of Sepals</div>
                            <div class="css_td">{{ data.NOSEP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Height of Pistil (cm)</div>
                            <div class="css_td">{{ data.HTPIS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Height of Stamen (cm)</div>
                            <div class="css_td">{{ data.HTSTA }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-4" name="accordion-1" type="checkbox" />
                <label for="ac-4">FRUIT DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Pod Color</div>
                            <div class="css_td">{{ data.PDCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pod length (cm)</div>
                            <div class="css_td">{{ data.PDLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pod width (cm)</div>
                            <div class="css_td">{{ data.PDWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of seeds per pod</div>
                            <div class="css_td">{{ data.NOSDSPD }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-5" name="accordion-1" type="checkbox" />
                <label for="ac-5">SEED DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Seed Color</div>
                            <div class="css_td">{{ data.SDCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">1000 seeds weight (gm)</div>
                            <div class="css_td">{{ data.SDWT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pollen fertility</div>
                            <div class="css_td">{{ data.POLFERT }}</div>
                        </div>
                    </div>
                </article>
            </div>
        </section>
    {% endblock %}
{% endembed %}