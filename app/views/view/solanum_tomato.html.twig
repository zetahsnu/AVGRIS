{% embed "base.html.twig" %}
    {% block javascript %}
        {{ parent() }}
        <script src="{{ BASE_PATH }}/public/js/search/view/back.js"></script>
    {% endblock %}
    {% block css %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ BASE_PATH }}/public/css/view/default.css">
    {% endblock %}
    {% block content %}
        {% embed "/view/part/title.html.twig" %}
        {% endembed %}
        {% embed "/view/part/header.html.twig" %}
        {% endembed %}
        <section class="ac-container">
            <div>
                <input id="ac-1" name="accordion-1" type="checkbox" />
                <label for="ac-1">SEEDLING DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Germination period</div>
                            <div class="css_td">{{ data.GERMPD }}</div>
                        </div>                        
                        <div class="css_tr">
                            <div class="css_td">Anthocyanin coloration of hypocotyl<br>(when the seedling primary leaves are fully opened and the terminal bud is around 5 mm)</div>
                            <div class="css_td">{{ data.ANTHO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stem pubescence</div>
                            <div class="css_td">{{ data.STPUB }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Primary leaf length (cm) (N=10)</div>
                            <div class="css_td">{{ data.PLFLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Primary leaf width (cm) (N=10)</div>
                            <div class="css_td">{{ data.PLFWID }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-2" name="accordion-1" type="checkbox" />
                <label for="ac-2">VEGETATIVE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Growth habit<br>(when the fruits of first truss ripen)</div>
                            <div class="css_td">{{ data.GHBT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of leaves under first inflorescence (N=5)</div>
                            <div class="css_td">{{ data.NOLEAVES }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf attitude</div>
                            <div class="css_td">{{ data.LFATT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf type</div>
                            <div class="css_td">{{ data.LFTYPE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Degree of leaf dissection</div>
                            <div class="css_td">{{ data.DEGLFDIS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Anthocyanin coloration of leaf veins</div>
                            <div class="css_td">{{ data.ACLFVEIN }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-3" name="accordion-1" type="checkbox" />
                <label for="ac-3">INFLORESCENCE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Days to flower<br>(no. of days from sowing until 50% of plants have the most flowers)</div>
                            <div class="css_td">{{ data.DAYSFLO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of flower per inflorescence (N=5)</div>
                            <div class="css_td">{{ data.NOFLOPINF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Inflorescence type<br>(information should be taken from the 2<sup>nd</sup> and 3<sup>rd</sup> truss)</div>
                            <div class="css_td">{{ data.INFTYPE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Petal length (cm) (N=5)</div>
                            <div class="css_td">{{ data.PETLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Sepal length (cm) (N=5)</div>
                            <div class="css_td">{{ data.SEPALEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Style type</div>
                            <div class="css_td">{{ data.STYPE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stamen length (cm) (N=5)</div>
                            <div class="css_td">{{ data.STLEN }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-4" name="accordion-1" type="checkbox" />
                <label for="ac-4">FRUIT DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Days to fruit<br>(no. of days from sowing until 50% of plants have the most fruits)</div>
                            <div class="css_td">{{ data.DAYSFRU }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit size variation within a plant</div>
                            <div class="css_td">{{ data.FRSZEV }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Predominant fruit shape<br>(after the fruits turn color)</div>
                            <div class="css_td">{{ data.PREDO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Exterior color of immature fruit</div>
                            <div class="css_td">{{ data.EXCOLIF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Intensity of greenback</div>
                            <div class="css_td">{{ data.INTGBK }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Presence of jointless pedicel</div>
                            <div class="css_td">{{ data.PJNTPED }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ribbing at calyx end</div>
                            <div class="css_td">{{ data.RCALYX }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Firmness</div>
                            <div class="css_td">{{ data.FIRM }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Radial cracking (environmental dependent)</div>
                            <div class="css_td">{{ data.RADIAL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Concentric cracking (environmental dependent)</div>
                            <div class="css_td">{{ data.CONCEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit fasciation</div>
                            <div class="css_td">{{ data.FRFAS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of fruit set per inflorescence (N=10; the 2<sup>nd</sup> truss)</div>
                            <div class="css_td">{{ data.NOFRUPI }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit weight (gm) (N=10)</div>
                            <div class="css_td">{{ data.FRWT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit length (cm) (N=10)</div>
                            <div class="css_td">{{ data.FRLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit width (cm) (N=10)</div>
                            <div class="css_td">{{ data.FRWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Exterior color of mature fruit</div>
                            <div class="css_td">{{ data.EXCOLMF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pedicel length (mm)<br>(N=10; measured from abscission layer to calyx)</div>
                            <div class="css_td">{{ data.PEDLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pedicel area</div>
                            <div class="css_td">{{ data.PEDAREA }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Size of pedicel scar width (mm) (N=10)</div>
                            <div class="css_td">{{ data.SZEPEDS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Size of corky area around pedicel scar (mm) (N=10)</div>
                            <div class="css_td">{{ data.SZECORKY }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Shape of pistil scar</div>
                            <div class="css_td">{{ data.SHPPS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Blossom end shape</div>
                            <div class="css_td">{{ data.BLENDS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Blossom end scar condition</div>
                            <div class="css_td">{{ data.BLESCON }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Interior flesh color (pericarp)</div>
                            <div class="css_td">{{ data.INTFCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Interior flesh color intensity</div>
                            <div class="css_td">{{ data.INTFCINT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Transverse section</div>
                            <div class="css_td">{{ data.TRANSEC }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Thickness of fruit wall (mm) (N=10)</div>
                            <div class="css_td">{{ data.TFRWL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Vascular bundle content</div>
                            <div class="css_td">{{ data.VBCONT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Color of core</div>
                            <div class="css_td">{{ data.COLCORE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of locules (N=10)</div>
                            <div class="css_td">{{ data.NOLOCU }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Thickness of pericarp (mm) (N=10)</div>
                            <div class="css_td">{{ data.THICKP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Size of core (mm)<br>(N=10; in cross-section)</div>
                            <div class="css_td">{{ data.SZECOR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Puffiness</div>
                            <div class="css_td">{{ data.PUFF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of pedicel scar (mm) (N=10)</div>
                            <div class="css_td">{{ data.LENPSCAR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Skin color of ripen fruit<br>(observe the peeled fruit skin)</div>
                            <div class="css_td">{{ data.SCOLRFR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Easiness of peeling</div>
                            <div class="css_td">{{ data.EPEEL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Soluble solids (%)</div>
                            <div class="css_td">{{ data.SSOLID }}</div>
                        </div>
                    </div>
                </article>
            </div>            
        </section>
    {% endblock %}
{% endembed %}