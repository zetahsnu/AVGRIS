{% embed "base.html.twig" %}
    {% block javascript %}
        {{ parent() }}
        <script src="{{ BASE_PATH }}/public/js/search/view/back.js"></script>
    {% endblock %}
    {% block css %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ BASE_PATH }}/public/css/view/default.css">
    {% endblock %}
    {% block content %}
        {% embed "/view/part/title.html.twig" %}
        {% endembed %}
        {% embed "/view/part/header.html.twig" %}
        {% endembed %}
        <section class="ac-container">
            <div>
                <input id="ac-1" name="accordion-1" type="checkbox" />
                <label for="ac-1">SEEDLING DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Hypocotyl color</div>
                            <div class="css_td">{{ data.HYCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Primary leaf length (cm) (N=10)</div>
                            <div class="css_td">{{ data.PLFLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Primary leaf width (cm) (N=10)</div>
                            <div class="css_td">{{ data.PLFWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Primary leaf shape</div>
                            <div class="css_td">{{ data.PLFSHP }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-2" name="accordion-1" type="checkbox" />
                <label for="ac-2">VEGETATIVE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Growth habit</div>
                            <div class="css_td">{{ data.GHABT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Lodging</div>
                            <div class="css_td">{{ data.LODGE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Terminal leaflet size</div>
                            <div class="css_td">{{ data.TLFSZE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Terminal leaflet shape</div>
                            <div class="css_td">{{ data.TLFSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf pubescence density</div>
                            <div class="css_td">{{ data.LFPDENSE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf pubescence length</div>
                            <div class="css_td">{{ data.LFPLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf color<br>(record at 50% flowering)</div>
                            <div class="css_td">{{ data.LFCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Petiole color</div>
                            <div class="css_td">{{ data.PETCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Color of basal petioles</div>
                            <div class="css_td">{{ data.COLBPET }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Days to 50% flowering<br>(no. of days from planting to 50% of plants with at least one open flower)</div>
                            <div class="css_td">{{ data.DAYSFLO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Plant height at 50% flowering (cm) (N=5)</div>
                            <div class="css_td">{{ data.PHT_FLO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Plant height at maturation (cm) (N=5)</div>
                            <div class="css_td">{{ data.PHT_MAT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of clusters (N=10)</div>
                            <div class="css_td">{{ data.CLUSNO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pods per plant (N=10)</div>
                            <div class="css_td">{{ data.PODS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Days to 50% maturity</div>
                            <div class="css_td">{{ data.DAYSMAT }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-3" name="accordion-1" type="checkbox" />
                <label for="ac-3">INFLORESCENCE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Raceme position<br>(record when the first pod changes color)</div>
                            <div class="css_td">{{ data.RACPOS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Calyx color</div>
                            <div class="css_td">{{ data.CLXCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Standard petal color</div>
                            <div class="css_td">{{ data.SPETCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Standard petal color density</div>
                            <div class="css_td">{{ data.SPETDEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Wings petal color</div>
                            <div class="css_td">{{ data.WPCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Wings petal color density</div>
                            <div class="css_td">{{ data.WPCDEN }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-4" name="accordion-1" type="checkbox" />
                <label for="ac-4">POD DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Pod color at mature stage</div>
                            <div class="css_td">{{ data.PCOLMS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pod pubescence number</div>
                            <div class="css_td">{{ data.PPUBNO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pod pubescence color</div>
                            <div class="css_td">{{ data.PPUBCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pod pubescence length</div>
                            <div class="css_td">{{ data.PPUBLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pod length (cm) (N=10)</div>
                            <div class="css_td">{{ data.PODLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seeds per pods (N=5)</div>
                            <div class="css_td">{{ data.SDPPOD }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-5" name="accordion-1" type="checkbox" />
                <label for="ac-5">SEED DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Seed coat color</div>
                            <div class="css_td">{{ data.SDCTCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Lustre on seed surface</div>
                            <div class="css_td">{{ data.LUSTRE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed hilum color</div>
                            <div class="css_td">{{ data.HILUM }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Thousand seed weight (gm) (seed weight of 1000 seeds at 9% moisture content)</div>
                            <div class="css_td">{{ data.SDWT }}</div>
                        </div>
                    </div>
                </article>
            </div>
        </section>
    {% endblock %}
{% endembed %}