{% embed "base.html.twig" %}
    {% block javascript %}
        {{ parent() }}
        <script src="{{ BASE_PATH }}/public/js/search/view/back.js"></script>
    {% endblock %}
    {% block css %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ BASE_PATH }}/public/css/view/default.css">
    {% endblock %}
    {% block content %}
        {% embed "/view/part/title.html.twig" %}
        {% endembed %}
        {% embed "/view/part/header.html.twig" %}
        {% endembed %}
        <section class="ac-container">
            <div>
                <input id="ac-1" name="accordion-1" type="checkbox" />
                <label for="ac-1">SEEDLING DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Germination period<br>(no. of days from sowing to first germination)</div>
                            <div class="css_td">{{ data.GERMPD }}</div>
                        </div>                        
                        <div class="css_tr">
                            <div class="css_td">Cotyledon color</div>
                            <div class="css_td">{{ data.COTCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Cotyledon shape</div>
                            <div class="css_td">{{ data.COTSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of cotyledon (mm)</div>
                            <div class="css_td">{{ data.LENCOT }}</div>
                        </div>                        
                        <div class="css_tr">
                            <div class="css_td">Width of cotyledon (mm)</div>
                            <div class="css_td">{{ data.WIDCOT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Cotyledon size</div>
                            <div class="css_td">{{ data.COTSZE }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-2" name="accordion-1" type="checkbox" />
                <label for="ac-2">VEGETATIVE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Plant vigor</div>
                            <div class="css_td">{{ data.PLVIG }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Plant type</div>
                            <div class="css_td">{{ data.PLTYP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of side shoots</div>
                            <div class="css_td">{{ data.NOSDSHT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Main vine length (cm)</div>
                            <div class="css_td">{{ data.MVINLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Main vine diameter (cm)<br>(between the inter-node of 4<sup>th</sup> and 5<sup>th</sup>)</div>
                            <div class="css_td">{{ data.MVINDIA }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of inter-node (cm)<br>(between 4<sup>th</sup> and 5<sup>th</sup> on main vine)</div>
                            <div class="css_td">{{ data.LENINT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Tendrils</div>
                            <div class="css_td">{{ data.TENDRIL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf color</div>
                            <div class="css_td">{{ data.LFCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf pubescence density</div>
                            <div class="css_td">{{ data.LFPUBDEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf length (cm)</div>
                            <div class="css_td">{{ data.LFLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf width (cm)</div>
                            <div class="css_td">{{ data.LFWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf size</div>
                            <div class="css_td">{{ data.LFSZE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of petiole (cm)</div>
                            <div class="css_td">{{ data.LENPET }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-3" name="accordion-1" type="checkbox" />
                <label for="ac-3">INFLORESCENCE DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Days to first male flower</div>
                            <div class="css_td">{{ data.DAYSFMF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Days to first female flower</div>
                            <div class="css_td">{{ data.DAYSFFF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Node number of first female flower</div>
                            <div class="css_td">{{ data.NODFFF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary shape</div>
                            <div class="css_td">{{ data.OVSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary color</div>
                            <div class="css_td">{{ data.OVCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary size</div>
                            <div class="css_td">{{ data.OVSZE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary length (cm)</div>
                            <div class="css_td">{{ data.OVLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Ovary diameter (cm)</div>
                            <div class="css_td">{{ data.OVDIA }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Time of flowering</div>
                            <div class="css_td">{{ data.TIMEFLO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Flower color</div>
                            <div class="css_td">{{ data.FLOCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Sex type</div>
                            <div class="css_td">{{ data.SEXTYPE }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-4" name="accordion-1" type="checkbox" />
                <label for="ac-4">FRUIT DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Days to harvesting at commercial stage</div>
                            <div class="css_td">{{ data.DAYSHARV }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit set</div>
                            <div class="css_td">{{ data.FRSET }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit shape</div>
                            <div class="css_td">{{ data.FRSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit type</div>
                            <div class="css_td">{{ data.FRTYP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stem-end shape</div>
                            <div class="css_td">{{ data.STESHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Blossom-end fruit shape</div>
                            <div class="css_td">{{ data.BLEFRSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Immature fruit color</div>
                            <div class="css_td">{{ data.IMFRCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit skin mottling</div>
                            <div class="css_td">{{ data.FRSKNMOT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit surface</div>
                            <div class="css_td">{{ data.FRSURF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fiber (ripened fruit)</div>
                            <div class="css_td">{{ data.FIBER }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit skin texture</div>
                            <div class="css_td">{{ data.FRSKNTEX }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit skin texture intensity</div>
                            <div class="css_td">{{ data.FRSKNTEXIN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit firmness (ripened fruit)</div>
                            <div class="css_td">{{ data.FRFIRM }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit stem color (commercial fruit)</div>
                            <div class="css_td">{{ data.FRSTCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Stripe on blossom-end of fruit</div>
                            <div class="css_td">{{ data.STRBLEFR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Color of stripes</div>
                            <div class="css_td">{{ data.COLSTR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit spine color</div>
                            <div class="css_td">{{ data.FRSPCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of fruit peduncle (cm)</div>
                            <div class="css_td">{{ data.LENPED }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Peduncle thickness (mm)</div>
                            <div class="css_td">{{ data.PEDTHIC }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit weight (gm)</div>
                            <div class="css_td">{{ data.FRWT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Wax on fruit</div>
                            <div class="css_td">{{ data.WAXFR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit length at table use maturity (cm)</div>
                            <div class="css_td">{{ data.FRLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit breadth at table use maturity (cm)</div>
                            <div class="css_td">{{ data.FRBRD }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of locules</div>
                            <div class="css_td">{{ data.NOLOCU }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Length of pistil scar (cm)</div>
                            <div class="css_td">{{ data.LENPSCAR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Breadth of pistil scar (cm)</div>
                            <div class="css_td">{{ data.BRDPSCAR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Flesh texture</div>
                            <div class="css_td">{{ data.FLTEX }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Flesh color</div>
                            <div class="css_td">{{ data.FLCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Flesh thickness (mm)</div>
                            <div class="css_td">{{ data.FLTHIC }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Rind thickness (mm)</div>
                            <div class="css_td">{{ data.RINDTHIC }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit aroma</div>
                            <div class="css_td">{{ data.FRARO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Fruit storage ability (without refrigeration)</div>
                            <div class="css_td">{{ data.FRSTOAB }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of seeds per fruit</div>
                            <div class="css_td">{{ data.NOSDSPF }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-5" name="accordion-1" type="checkbox" />
                <label for="ac-5">SEED DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Seed coat color</div>
                            <div class="css_td">{{ data.SDCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed shape</div>
                            <div class="css_td">{{ data.SDSHP }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed length (mm)</div>
                            <div class="css_td">{{ data.SDLEN }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed width (mm)</div>
                            <div class="css_td">{{ data.SDWID }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seed thickness (mm)</div>
                            <div class="css_td">{{ data.SDTHIC }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">100 seeds weight (gm)</div>
                            <div class="css_td">{{ data.SDWT }}</div>
                        </div>
                    </div>
                </article>
            </div>
        </section>
    {% endblock %}
{% endembed %}