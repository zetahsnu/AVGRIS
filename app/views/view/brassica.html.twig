{% embed "base.html.twig" %}
    {% block javascript %}
        {{ parent() }}
        <script src="{{ BASE_PATH }}/public/js/search/view/back.js"></script>
    {% endblock %}
    {% block css %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ BASE_PATH }}/public/css/view/default.css">
    {% endblock %}
    {% block content %}
        {% embed "/view/part/title.html.twig" %}
        {% endembed %}
        {% embed "/view/part/header.html.twig" %}
        {% endembed %}
        <section class="ac-container">
            <div>
                <input id="ac-1" name="accordion-1" type="checkbox" />
                <label for="ac-1">SEEDLING DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Hypocotyl color</div>
                            <div class="css_td">{{ data.HYPCOL }}</div>
                        </div>                        
                        <div class="css_tr">
                            <div class="css_td">Seedling leaf color</div>
                            <div class="css_td">{{ data.SDLFCOL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Margin of seedling leaf</div>
                            <div class="css_td">{{ data.MGNSDLF }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Seedling pubescence</div>
                            <div class="css_td">{{ data.SDPUB }}</div>
                        </div>
                    </div>
                </article>
            </div>
            <div>
                <input id="ac-2" name="accordion-1" type="checkbox" />
                <label for="ac-2">VEGETATIVE DATA (Leaf)</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Plant growth habit</div>
                            <div class="css_td">{{ data.PLGHBT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Plant height (cm) – measure extremity of plant</div>
                            <div class="css_td">{{ data.PLHT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Plant diameter (cm) – measure extremity of plant</div>
                            <div class="css_td">{{ data.PLDTR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Lodging</div>
                            <div class="css_td">{{ data.LDG }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Number of leaves (estimated)</div>
                            <div class="css_td">{{ data.NOLEAVE }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Leaf length (cm)<br>(largest leaf including petiole)</div>
