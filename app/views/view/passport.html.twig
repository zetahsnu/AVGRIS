                        <div class="css_tr">
                            <div class="css_td">Collecting institute</div>
                            <div class="css_td">{{ data.COINST }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Collection date of original sample</div>
                            <div class="css_td">{{ data.CODATE }}</div>
