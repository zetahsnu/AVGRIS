{% embed "base.html.twig" %}
    {% block javascript %}
        {{ parent() }}
        <script src="{{ BASE_PATH }}/public/js/search/view/back.js"></script>
    {% endblock %}
    {% block css %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ BASE_PATH }}/public/css/view/default.css">
    {% endblock %}
    {% block content %}
        {% embed "/view/part/title.html.twig" %}
        {% endembed %}
        <section class="ac-container">
            <div>
                <input id="ac-1" name="accordion-1" type="checkbox" checked />
                <label for="ac-1">PASSPORT DATA</label>
                <article>
                    <div class="css_table">
                        <div class="css_tr">
                            <div class="css_td">Vegetable introduction number</div>
                            <div class="css_td">{{ data.VINO }}</div>
                        </div>                        
                        <div class="css_tr">
                            <div class="css_td">Crop accession number</div>
                            <div class="css_td">{{ data.ACCNO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Duplicate note</div>
                            <div class="css_td">{{ data.DUPLIC }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Temporary number</div>
                            <div class="css_td">{{ data.TEMPNO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Variant</div>
                            <div class="css_td">{{ data.VARIANT }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Distribution status</div>
                            <div class="css_td">{{ data.DISSTA }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Family</div>
                            <div class="css_td">{{ data.FAMILY }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Genus</div>
                            <div class="css_td">{{ data.GENUS }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Species</div>
                            <div class="css_td">{{ data.SPECIES }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Subtaxa</div>
                            <div class="css_td">{{ data.SUBTAXA }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Sender's name</div>
                            <div class="css_td">{{ data.SENAME }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Donor identification number</div>
                            <div class="css_td">{{ data.DONO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Other number 1</div>
                            <div class="css_td">{{ data.FIRNO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Other number 2</div>
                            <div class="css_td">{{ data.SECNO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Other number 3</div>
                            <div class="css_td">{{ data.TRDNO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">PI number</div>
                            <div class="css_td">{{ data.PINO }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Pedigree / Cultivar name</div>
                            <div class="css_td">{{ data.PEDCUL }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Donor institute</div>
                            <div class="css_td">{{ data.DOINST }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Donor's address</div>
                            <div class="css_td">{{ data.DOADDR }}</div>
                        </div>
                        <div class="css_tr">
                            <div class="css_td">Acquisition date</div>
                            <div class="css_td">{{ data.ACQDAT }}</div>
