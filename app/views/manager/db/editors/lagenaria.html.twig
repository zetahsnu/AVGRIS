<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>FRUIT DATA</span></a></li>
            <li><a href="#section-line-5"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='COTSZE'><div>Cotyledon size:</div><div>
                    <select>
                        <option value="">* COTSZE *</option>
                        {% for item in related.COTSZE %}
                            <option value="{{ item.REFNO }}">{{ item.COTSZE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COTCOL'><div>Cotyledon color:</div><div>
                    <select>
                        <option value="">* COTCOL *</option>
                        {% for item in related.COTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.COTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='MVINLEN'><div>Main vine length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="MVINLEN" /></div></div>
            <div data-type='MVINDIA'><div>Main vine diameter (cm):</div><div><input type="number" step="0.01" min="0" placeholder="MVINDIA" /></div></div>
            <div data-type='INLEN'><div>Internode length (in cm; main vine 7<sup>th</sup> - 8<sup>th</sup>):</div><div><input type="number" step="0.01" min="0" placeholder="INLEN" /></div></div>
            <div data-type='DPLFLOB'><div>Depth of leaf lobes:</div><div>
                    <select>
                        <option value="">* DPLFLOB *</option>
                        {% for item in related.DPLFLOB %}
                            <option value="{{ item.REFNO }}">{{ item.DPLFLOB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFLEN'><div>Leaf length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFLEN" /></div></div>
            <div data-type='LFWID'><div>Leaf width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFWID" /></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PETLEN'><div>Petiole length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PETLEN" /></div></div>
            <div data-type='LFPUB'><div>Leaf pubescence:</div><div>
                    <select>
                        <option value="">* LFPUB *</option>
                        {% for item in related.LFPUB %}
                            <option value="{{ item.REFNO }}">{{ item.LFPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLGRO'><div>Plant growth:</div><div>
                    <select>
                        <option value="">* PLGRO *</option>
                        {% for item in related.PLGRO %}
                            <option value="{{ item.REFNO }}">{{ item.PLGRO }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSOW'><div>Days from sowing to first female flower:</div><div><input type="number" step="1" min="0" placeholder="DAYSOW" /></div></div>
            <div data-type='NODFFEM'><div>No. of the node of first female flower:</div><div><input type="number" step="0.01" min="0" placeholder="NODFFEM" /></div></div>
            <div data-type='OVLEN'><div>Ovary length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="OVLEN" /></div></div>
            <div data-type='OVWID'><div>Ovary width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="OVWID" /></div></div>
            <div data-type='OVSHP'><div>Ovary shape:</div><div>
                    <select>
                        <option value="">* OVSHP *</option>
                        {% for item in related.OVSHP %}
                            <option value="{{ item.REFNO }}">{{ item.OVSHP }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='FRSETHBT'><div>Fruit set habit:</div><div>
                    <select>
                        <option value="">* FRSETHBT *</option>
                        {% for item in related.FRSETHBT %}
                            <option value="{{ item.REFNO }}">{{ item.FRSETHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SEQFRU'><div>Sequential fruiting:</div><div>
                    <select>
                        <option value="">* SEQFRU *</option>
                        {% for item in related.SEQFRU %}
                            <option value="{{ item.REFNO }}">{{ item.SEQFRU }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLOTCOMFR'><div>From flowering to first harvesting commercial fruit (days):</div><div><input type="number" step="0.01" min="0" placeholder="FLOTCOMFR" /></div></div>
            <div data-type='SOWTCOMFR'><div>From sowing to first harvesting commercial fruit (days):</div><div><input type="number" step="0.01" min="0" placeholder="SOWTCOMFR" /></div></div>
            <div data-type='FRSHP'><div>Fruit shape:</div><div>
                    <select>
                        <option value="">* FRSHP *</option>
                        {% for item in related.FRSHP %}
                            <option value="{{ item.REFNO }}">{{ item.FRSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRWT'><div>Fruit weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="FRWT" /></div></div>
            <div data-type='FRLEN'><div>Fruit length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRLEN" /></div></div>
            <div data-type='FRDIA'><div>Fruit diameter (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRDIA" /></div></div>
            <div data-type='FRPED'><div>Fruit peduncle length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRPED" /></div></div>
            <div data-type='FRSKTHIC'><div>Fruit skin thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="FRSKTHIC" /></div></div>
            <div data-type='FRFLTHIC'><div>Fruit flesh thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="FRFLTHIC" /></div></div>
            <div data-type='FRCAVDIA'><div>Fruit cavity diameter (mm):</div><div><input type="number" step="0.01" min="0" placeholder="FRCAVDIA" /></div></div>
            <div data-type='FRSKNSUR'><div>Fruit skin surface:</div><div>
                    <select>
                        <option value="">* FRSKNSUR *</option>
                        {% for item in related.FRSKNSUR %}
                            <option value="{{ item.REFNO }}">{{ item.FRSKNSUR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSKNCOL'><div>Fruit skin color:</div><div>
                    <select>
                        <option value="">* FRSKNCOL *</option>
                        {% for item in related.FRSKNCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FRSKNCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSKNSTR'><div>Fruit skin streak:</div><div>
                    <select>
                        <option value="">* FRSKNSTR *</option>
                        {% for item in related.FRSKNSTR %}
                            <option value="{{ item.REFNO }}">{{ item.FRSKNSTR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSKSTCOC'><div>Fruit skin streak color:</div><div>
                    <select>
                        <option value="">* FRSKSTCOC *</option>
                        {% for item in related.FRSKSTCOC %}
                            <option value="{{ item.REFNO }}">{{ item.FRSKSTCOC }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSKCOL'><div>Fruit skin color (at physiological ripening stage):</div><div>
                    <select>
                        <option value="">* FRSKCOL *</option>
                        {% for item in related.FRSKCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FRSKCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSKSTCOP'><div>Fruit skin streak color (at physiological ripening stage):</div><div>
                    <select>
                        <option value="">* FRSKSTCOP *</option>
                        {% for item in related.FRSKSTCOP %}
                            <option value="{{ item.REFNO }}">{{ item.FRSKSTCOP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRWTPHY'><div>Fruit weight (gm; at physiological ripening stage):</div><div><input type="number" step="0.01" min="0" placeholder="FRWTPHY" /></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SDSHP *</option>
                        {% for item in related.SDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.SDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDLEN'><div>Seed length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDLEN" /></div></div>
            <div data-type='SDWID'><div>Seed width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWID" /></div></div>
            <div data-type='SDTHIC'><div>Seed thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDTHIC" /></div></div>
            <div data-type='SDCTCOL'><div>Seed coat color:</div><div>
                    <select>
                        <option value="">* SDCTCOL *</option>
                        {% for item in related.SDCTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSTR'><div>Seed streaks:</div><div>
                    <select>
                        <option value="">* SDSTR *</option>
                        {% for item in related.SDSTR %}
                            <option value="{{ item.REFNO }}">{{ item.SDSTR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOSDSPF'><div>Number of seeds per fruit:</div><div><input type="number" step="0.01" min="0" placeholder="NOSDSPF" /></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->