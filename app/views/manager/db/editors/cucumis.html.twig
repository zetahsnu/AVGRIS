<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>FRUIT DATA</span></a></li>
            <li><a href="#section-line-5"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='COTCOL'><div>Cotyledon color:</div><div>
                    <select>
                        <option value="">* COTCOL *</option>
                        {% for item in related.COTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.COTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COTSHP'><div>Cotyledon shape:</div><div>
                    <select>
                        <option value="">* COTSHP *</option>
                        {% for item in related.COTSHP %}
                            <option value="{{ item.REFNO }}">{{ item.COTSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENCOT'><div>Length of cotyledon (mm):</div><div><input type="number" step="0.01" min="0" placeholder="LENCOT" /></div></div>
            <div data-type='WIDCOT'><div>Width of cotyledon (mm):</div><div><input type="number" step="0.01" min="0" placeholder="WIDCOT" /></div></div>
            <div data-type='COTSZE'><div>Cotyledon size:</div><div>
                    <select>
                        <option value="">* COTSZE *</option>
                        {% for item in related.COTSZE %}
                            <option value="{{ item.REFNO }}">{{ item.COTSZE }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='PLVIG'><div>Plant vigor:</div><div>
                    <select>
                        <option value="">* PLVIG *</option>
                        {% for item in related.PLVIG %}
                            <option value="{{ item.REFNO }}">{{ item.PLVIG }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLTYP'><div>Plant type:</div><div>
                    <select>
                        <option value="">* PLTYP *</option>
                        {% for item in related.PLTYP %}
                            <option value="{{ item.REFNO }}">{{ item.PLTYP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOSDSHT'><div>Number of side shoots:</div><div>
                    <select>
                        <option value="">* NOSDSHT *</option>
                        {% for item in related.NOSDSHT %}
                            <option value="{{ item.REFNO }}">{{ item.NOSDSHT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='MVINLEN'><div>Main vine length (cm):</div><div>
                    <select>
                        <option value="">* MVINLEN *</option>
                        {% for item in related.MVINLEN %}
                            <option value="{{ item.REFNO }}">{{ item.MVINLEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='MVINDIA'><div>Main vine diameter (cm; between the inter-node of 4<sup>th</sup> and 5<sup>th</sup>):</div><div><input type="number" step="0.01" min="0" placeholder="MVINDIA" /></div></div>
            <div data-type='LENINT'><div>Length of inter-node (cm; between <sup>th</sup> and 5<sup>th</sup> on main vine):</div><div>
                    <select>
                        <option value="">* LENINT *</option>
                        {% for item in related.LENINT %}
                            <option value="{{ item.REFNO }}">{{ item.LENINT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TENDRIL'><div>Tendrils:</div><div>
                    <select>
                        <option value="">* TENDRIL *</option>
                        {% for item in related.TENDRIL %}
                            <option value="{{ item.REFNO }}">{{ item.TENDRIL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFPUBDEN'><div>Leaf pubescence density:</div><div>
                    <select>
                        <option value="">* LFPUBDEN *</option>
                        {% for item in related.LFPUBDEN %}
                            <option value="{{ item.REFNO }}">{{ item.LFPUBDEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFLEN'><div>Leaf length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFLEN" /></div></div>
            <div data-type='LFWID'><div>Leaf width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFWID" /></div></div>
            <div data-type='LFSZE'><div>Leaf size:</div><div>
                    <select>
                        <option value="">* LFSZE *</option>
                        {% for item in related.LFSZE %}
                            <option value="{{ item.REFNO }}">{{ item.LFSZE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENPET'><div>Length of petiole (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPET" /></div></div>            
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFMF'><div>Days to first male flower:</div><div><input type="number" step="1" min="0" placeholder="DAYSFMF" /></div></div>
            <div data-type='DAYSFFF'><div>Days to first female flower:</div><div><input type="number" step="1" min="0" placeholder="DAYSFFF" /></div></div>
            <div data-type='NODFFF'><div>Node number of first female flower:</div><div><input type="number" step="0.01" min="0" placeholder="NODFFF" /></div></div>
            <div data-type='OVSHP'><div>Ovary shape:</div><div>
                    <select>
                        <option value="">* OVSHP *</option>
                        {% for item in related.OVSHP %}
                            <option value="{{ item.REFNO }}">{{ item.OVSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='OVCOL'><div>Ovary color:</div><div>
                    <select>
                        <option value="">* OVCOL *</option>
                        {% for item in related.OVCOL %}
                            <option value="{{ item.REFNO }}">{{ item.OVCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='OVSZE'><div>Ovary size:</div><div>
                    <select>
                        <option value="">* OVSZE *</option>
                        {% for item in related.OVSZE %}
                            <option value="{{ item.REFNO }}">{{ item.OVSZE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='OVLEN'><div>Ovary length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="OVLEN" /></div></div>
            <div data-type='OVDIA'><div>Ovary diameter (cm):</div><div><input type="number" step="0.01" min="0" placeholder="OVDIA" /></div></div>
            <div data-type='TIMEFLO'><div>Time of flowering:</div><div>
                    <select>
                        <option value="">* TIMEFLO *</option>
                        {% for item in related.TIMEFLO %}
                            <option value="{{ item.REFNO }}">{{ item.TIMEFLO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLOCOL'><div>Flower color:</div><div>
                    <select>
                        <option value="">* FLOCOL *</option>
                        {% for item in related.FLOCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLOCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SEXTYPE'><div>Sex type:</div><div>
                    <select>
                        <option value="">* SEXTYPE *</option>
                        {% for item in related.SEXTYPE %}
                            <option value="{{ item.REFNO }}">{{ item.SEXTYPE }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='DAYSHARV'><div>Days to harvesting at commercial stage:</div><div><input type="number" step="1" min="0" placeholder="DAYSHARV" /></div></div>
            <div data-type='FRSET'><div>Fruit set:</div><div>
                    <select>
                        <option value="">* FRSET *</option>
                        {% for item in related.FRSET %}
                            <option value="{{ item.REFNO }}">{{ item.FRSET }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSHP'><div>Fruit shape:</div><div>
                    <select>
                        <option value="">* FRSHP *</option>
                        {% for item in related.FRSHP %}
                            <option value="{{ item.REFNO }}">{{ item.FRSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRTYP'><div>Fruit type:</div><div>
                    <select>
                        <option value="">* FRTYP *</option>
                        {% for item in related.FRTYP %}
                            <option value="{{ item.REFNO }}">{{ item.FRTYP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STESHP'><div>Stem-end shape:</div><div>
                    <select>
                        <option value="">* STESHP *</option>
                        {% for item in related.STESHP %}
                            <option value="{{ item.REFNO }}">{{ item.STESHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BLEFRSHP'><div>Blossom-end fruit shape:</div><div>
                    <select>
                        <option value="">* BLEFRSHP *</option>
                        {% for item in related.BLEFRSHP %}
                            <option value="{{ item.REFNO }}">{{ item.BLEFRSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='IMFRCOL'><div>Immature fruit color:</div><div>
                    <select>
                        <option value="">* IMFRCOL *</option>
                        {% for item in related.IMFRCOL %}
                            <option value="{{ item.REFNO }}">{{ item.IMFRCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSKNMOT'><div>Fruit skin mottling:</div><div>
                    <select>
                        <option value="">* FRSKNMOT *</option>
                        {% for item in related.FRSKNMOT %}
                            <option value="{{ item.REFNO }}">{{ item.FRSKNMOT }}</option>
                        {% endfor %}
                    </select>
                </div></div>
            <div data-type='FRSURF'><div>Fruit surface:</div><div>
                    <select>
                        <option value="">* FRSURF *</option>
                        {% for item in related.FRSURF %}
                            <option value="{{ item.REFNO }}">{{ item.FRSURF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FIBER'><div>Fiber (ripened fruit):</div><div>
                    <select>
                        <option value="">* FIBER *</option>
                        {% for item in related.FIBER %}
                            <option value="{{ item.REFNO }}">{{ item.FIBER }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSKNTEX'><div>Fruit skin texture:</div><div>
                    <select>
                        <option value="">* FRSKNTEX *</option>
                        {% for item in related.FRSKNTEX %}
                            <option value="{{ item.REFNO }}">{{ item.FRSKNTEX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSKNTEXIN'><div>Fruit skin texture intensity:</div><div>
                    <select>
                        <option value="">* FRSKNTEXIN *</option>
                        {% for item in related.FRSKNTEXIN %}
                            <option value="{{ item.REFNO }}">{{ item.FRSKNTEXIN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRFIRM'><div>Fruit firmness (ripened fruit):</div><div>
                    <select>
                        <option value="">* FRFIRM *</option>
                        {% for item in related.FRFIRM %}
                            <option value="{{ item.REFNO }}">{{ item.FRFIRM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSTCOL'><div>Fruit stem color (commercial fruit):</div><div>
                    <select>
                        <option value="">* FRSTCOL *</option>
                        {% for item in related.FRSTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FRSTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STRBLEFR'><div>Stripe on blossom-end of fruit:</div><div>                
                    <select>
                        <option value="">* STRBLEFR *</option>
                        {% for item in related.STRBLEFR %}
                            <option value="{{ item.REFNO }}">{{ item.STRBLEFR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COLSTR'><div>Color of stripes:</div><div>
                    <select>
                        <option value="">* COLSTR *</option>
                        {% for item in related.COLSTR %}
                            <option value="{{ item.REFNO }}">{{ item.COLSTR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSPCOL'><div>Fruit spine color:</div><div>
                    <select>
                        <option value="">* FRSPCOL *</option>
                        {% for item in related.FRSPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FRSPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENPED'><div>Length of fruit peduncle (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPED" /></div></div>
            <div data-type='PEDTHIC'><div>Peduncle thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="PEDTHIC" /></div></div>
            <div data-type='FRWT'><div>Fruit weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="FRWT" /></div></div>
            <div data-type='WAXFR'><div>Wax on fruit:</div><div>
                    <select>
                        <option value="">* WAXFR *</option>
                        {% for item in related.WAXFR %}
                            <option value="{{ item.REFNO }}">{{ item.WAXFR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRLEN'><div>Fruit length at table use maturity (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRLEN" /></div></div>
            <div data-type='FRBRD'><div>Fruit breadth at table use maturity (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRBRD" /></div></div>
            <div data-type='NOLOCU'><div>Number of locules:</div><div><input type="number" step="0.01" min="0" placeholder="NOLOCU" /></div></div>
            <div data-type='LENPSCAR'><div>Length of pistil scar (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPSCAR" /></div></div>
            <div data-type='BRDPSCAR'><div>Breadth of pistil scar (cm):</div><div><input type="number" step="0.01" min="0" placeholder="BRDPSCAR" /></div></div>
            <div data-type='FLTEX'><div>Flesh texture:</div><div>
                    <select>
                        <option value="">* FLTEX *</option>
                        {% for item in related.FLTEX %}
                            <option value="{{ item.REFNO }}">{{ item.FLTEX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLCOL'><div>Flesh color:</div><div>
                    <select>
                        <option value="">* FLCOL *</option>
                        {% for item in related.FLCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLTHIC'><div>Flesh thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="FLTHIC" /></div></div>
            <div data-type='RINDTHIC'><div>Rind thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="RINDTHIC" /></div></div>
            <div data-type='FRARO'><div>Fruit aroma:</div><div>
                    <select>
                        <option value="">* FRARO *</option>
                        {% for item in related.FRARO %}
                            <option value="{{ item.REFNO }}">{{ item.FRARO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSTOAB'><div>Fruit storage ability (without refrigeration):</div><div>
                    <select>
                        <option value="">* FRSTOAB *</option>
                        {% for item in related.FRSTOAB %}
                            <option value="{{ item.REFNO }}">{{ item.FRSTOAB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOSDSPF'><div>Number of seeds per fruit:</div><div><input type="number" step="0.01" min="0" placeholder="NOSDSPF" /></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='SDCOL'><div>Seed coat color:</div><div>
                    <select>
                        <option value="">* SDCOL *</option>
                        {% for item in related.SDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SDSHP *</option>
                        {% for item in related.SDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.SDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDLEN'><div>Seed length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDLEN" /></div></div>
            <div data-type='SDWID'><div>Seed width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWID" /></div></div>
            <div data-type='SDTHIC'><div>Seed thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDTHIC" /></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->