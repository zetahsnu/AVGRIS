<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>            
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-4"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='ANTCOHYP'><div>Anthocyanin coloration of hypocotyl:</div><div>
                    <select>
                        <option value="">* ANTCOHYP *</option>
                        {% for item in related.ANTCOHYP %}
                            <option value="{{ item.REFNO }}">{{ item.ANTCOHYP }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='PLGRHBT'><div>Plant growth habit:</div><div>
                    <select>
                        <option value="">* PLGRHBT *</option>
                        {% for item in related.PLGRHBT %}
                            <option value="{{ item.REFNO }}">{{ item.PLGRHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STCOL'><div>Stem color:</div><div>
                    <select>
                        <option value="">* STCOL *</option>
                        {% for item in related.STCOL %}
                            <option value="{{ item.REFNO }}">{{ item.STCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='MATLFLEN'><div>Mature leaf length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="MATLFLEN" /></div></div>
            <div data-type='MATLFWID'><div>Mature leaf width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="MATLFWID" /></div></div>
            <div data-type='LFBLTAN'><div>Leaf blade tip angle:</div><div>
                    <select>
                        <option value="">* LFBLTAN *</option>
                        {% for item in related.LFBLTAN %}
                            <option value="{{ item.REFNO }}">{{ item.LFBLTAN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENPET'><div>Length of petiole (mm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPET" /></div></div>
            <div data-type='WIDPET'><div>Width of petiole (mm):</div><div><input type="number" step="0.01" min="0" placeholder="WIDPET" /></div></div>
            <div data-type='LFSHP'><div>Leaf shape:</div><div>
                    <select>
                        <option value="">* LFSHP *</option>
                        {% for item in related.LFSHP %}
                            <option value="{{ item.REFNO }}">{{ item.LFSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFPIG'><div>Leaf pigmentation:</div><div>
                    <select>
                        <option value="">* LFPIG *</option>
                        {% for item in related.LFPIG %}
                            <option value="{{ item.REFNO }}">{{ item.LFPIG }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFLO'><div>Days to flower:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='LENPED'><div>Length of pedicel (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPED" /></div></div>
            <div data-type='WIDPED'><div>Width of pedicel (mm):</div><div><input type="number" step="0.01" min="0" placeholder="WIDPED" /></div></div>
            <div data-type='PEDCOL'><div>Pedicel color:</div><div>
                    <select>
                        <option value="">* PEDCOL *</option>
                        {% for item in related.PEDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PEDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NUMPET'><div>Number of petal:</div><div><input type="number" step="0.01" min="0" placeholder="NUMPET" /></div></div>
            <div data-type='PETCOL'><div>Petal color:</div><div>
                    <select>
                        <option value="">* PETCOL *</option>
                        {% for item in related.PETCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PETCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HTINFLO'><div>Height of inflorescence (mm):</div><div><input type="number" step="0.01" min="0" placeholder="HTINFLO" /></div></div>
            <div data-type='WIDINFLO'><div>Width of inflorescence (mm):</div><div><input type="number" step="0.01" min="0" placeholder="WIDINFLO" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-4">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->