<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>            
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>FRUIT DATA</span></a></li>
            <li><a href="#section-line-5"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='HYPCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='GRHBT'><div>Growth habit:</div><div>
                    <select>
                        <option value="">* GRHBT *</option>
                        {% for item in related.GRHBT %}
                            <option value="{{ item.REFNO }}">{{ item.GRHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BRAN'><div>Branching:</div><div>
                    <select>
                        <option value="">* BRAN *</option>
                        {% for item in related.BRAN %}
                            <option value="{{ item.REFNO }}">{{ item.BRAN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFLSHP'><div>Leaflet shape:</div><div>
                    <select>
                        <option value="">* LFLSHP *</option>
                        {% for item in related.LFLSHP %}
                            <option value="{{ item.REFNO }}">{{ item.LFLSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFSFWX'><div>Leaf surface wax:</div><div>
                    <select>
                        <option value="">* LFSFWX *</option>
                        {% for item in related.LFSFWX %}
                            <option value="{{ item.REFNO }}">{{ item.LFSFWX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='GCOLINT'><div>Green color intensity of plant:</div><div>
                    <select>
                        <option value="">* GCOLINT *</option>
                        {% for item in related.GCOLINT %}
                            <option value="{{ item.REFNO }}">{{ item.GCOLINT }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='FLOCOL'><div>Flower color:</div><div>
                    <select>
                        <option value="">* FLOCOL *</option>
                        {% for item in related.FLOCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLOCOL }}</option>
                        {% endfor %}
                    </select>
                </div></div>
            <div data-type='SOWTOF'><div>From sowing to flowering (days):</div><div><input type="number" step="1" min="0" placeholder="SOWTOF" /></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='PDNOINF'><div>Pod number per inflorescence:</div><div>
                    <select>
                        <option value="">* PDNOINF *</option>
                        {% for item in related.PDNOINF %}
                            <option value="{{ item.REFNO }}">{{ item.PDNOINF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDLEN'><div>Pod length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDLEN" /></div></div>
            <div data-type='PDWID'><div>Pod width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDWID" /></div></div>
            <div data-type='PDTHIC'><div>Pod thickness (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDTHIC" /></div></div>
            <div data-type='PDAPX'><div>Pod apex:</div><div>
                    <select>
                        <option value="">* PDAPX *</option>
                        {% for item in related.PDAPX %}
                            <option value="{{ item.REFNO }}">{{ item.PDAPX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDSHP'><div>Pod shape:</div><div>
                    <select>
                        <option value="">* PDSHP *</option>
                        {% for item in related.PDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.PDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDSURF'><div>Pod surface:</div><div>
                    <select>
                        <option value="">* PDSURF *</option>
                        {% for item in related.PDSURF %}
                            <option value="{{ item.REFNO }}">{{ item.PDSURF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDCOL'><div>Pod color:</div><div>
                    <select>
                        <option value="">* PDCOL *</option>
                        {% for item in related.PDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDNOP'><div>Seed number per pod:</div><div><input type="number" step="0.01" min="0" placeholder="SDNOP" /></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='MSDLEN'><div>Matured seed length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="MSDLEN" /></div></div>
            <div data-type='MSDWID'><div>Matured seed width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="MSDWID" /></div></div>
            <div data-type='MSDTHIC'><div>Matured seed thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="MSDTHIC" /></div></div>
            <div data-type='SDSURF'><div>Seed surface:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='GDCOLSD'><div>Ground color of the seed:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COLDT'><div>Color dots:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HILCOL'><div>Hilum color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->