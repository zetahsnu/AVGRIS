<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>            
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>POD DATA</span></a></li>
            <li><a href="#section-line-5"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='HYCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYCOL *</option>
                        {% for item in related.HYCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLFLEN'><div>Primary leaf length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLFLEN" /></div></div>
            <div data-type='PLFWID'><div>Primary leaf width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLFWID" /></div></div>
            <div data-type='PLFSHP'><div>Primary leaf shape:</div><div>
                    <select>
                        <option value="">* PLFSHP *</option>
                        {% for item in related.PLFSHP %}
                            <option value="{{ item.REFNO }}">{{ item.PLFSHP }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='GHABT'><div>Growth habit:</div><div>
                    <select>
                        <option value="">* GHABT *</option>
                        {% for item in related.GHABT %}
                            <option value="{{ item.REFNO }}">{{ item.GHABT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LODGE'><div>Lodging:</div><div>
                    <select>
                        <option value="">* LODGE *</option>
                        {% for item in related.LODGE %}
                            <option value="{{ item.REFNO }}">{{ item.LODGE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TLFSZE'><div>Terminal leaflet size:</div><div>
                    <select>
                        <option value="">* TLFSZE *</option>
                        {% for item in related.TLFSZE %}
                            <option value="{{ item.REFNO }}">{{ item.TLFSZE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TLFSHP'><div>Terminal leaflet shape:</div><div>
                    <select>
                        <option value="">* TLFSHP *</option>
                        {% for item in related.TLFSHP %}
                            <option value="{{ item.REFNO }}">{{ item.TLFSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFPDENSE'><div>Leaf pubescence density:</div><div>
                    <select>
                        <option value="">* LFPDENSE *</option>
                        {% for item in related.LFPDENSE %}
                            <option value="{{ item.REFNO }}">{{ item.LFPDENSE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFPLEN'><div>Leaf pubescence length:</div><div>
                    <select>
                        <option value="">* LFPLEN *</option>
                        {% for item in related.LFPLEN %}
                            <option value="{{ item.REFNO }}">{{ item.LFPLEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFCOL'><div>Leaf color (record at 50% flowering):</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PETCOL'><div>Petiole color:</div><div>
                    <select>
                        <option value="">* PETCOL *</option>
                        {% for item in related.PETCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PETCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COLBPET'><div>Color of basal petioles:</div><div>
                    <select>
                        <option value="">* COLBPET *</option>
                        {% for item in related.COLBPET %}
                            <option value="{{ item.REFNO }}">{{ item.COLBPET }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='DAYSFLO'><div>Days to 50% flowering:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='PHT_FLO'><div>Plant height at 50% flowering (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PHT_FLO" /></div></div>
            <div data-type='PHT_MAT'><div>Plant height at maturation (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PHT_MAT" /></div></div>
            <div data-type='CLUSNO'><div>Number of clusters:</div><div><input type="number" step="0.01" min="0" placeholder="CLUSNO" /></div></div>
            <div data-type='PODS'><div>Pods per plant:</div><div><input type="number" step="0.01" min="0" placeholder="PODS" /></div></div>
            <div data-type='DAYSMAT'><div>Days to 50% maturity:</div><div><input type="number" step="1" min="0" placeholder="DAYSMAT" /></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='RACPOS'><div>Raceme position (record when the first pod changes color):</div><div>
                    <select>
                        <option value="">* RACPOS *</option>
                        {% for item in related.RACPOS %}
                            <option value="{{ item.REFNO }}">{{ item.RACPOS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='CLXCOL'><div>Calyx color:</div><div>
                    <select>
                        <option value="">* CLXCOL *</option>
                        {% for item in related.CLXCOL %}
                            <option value="{{ item.REFNO }}">{{ item.CLXCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SPETCOL'><div>Standard petal color:</div><div>
                    <select>
                        <option value="">* SPETCOL *</option>
                        {% for item in related.SPETCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SPETCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SPETDEN'><div>Standard petal color density:</div><div>
                    <select>
                        <option value="">* SPETDEN *</option>
                        {% for item in related.SPETDEN %}
                            <option value="{{ item.REFNO }}">{{ item.SPETDEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='WPCOL'><div>Wings petal color:</div><div>
                    <select>
                        <option value="">* WPCOL *</option>
                        {% for item in related.WPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.WPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='WPCDEN'><div>Wings petal color density:</div><div>
                    <select>
                        <option value="">* WPCDEN *</option>
                        {% for item in related.WPCDEN %}
                            <option value="{{ item.REFNO }}">{{ item.WPCDEN }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='PCOLMS'><div>Pod color at mature stage:</div><div>
                    <select>
                        <option value="">* PCOLMS *</option>
                        {% for item in related.PCOLMS %}
                            <option value="{{ item.REFNO }}">{{ item.PCOLMS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PPUBNO'><div>Pod pubescence number:</div><div>
                    <select>
                        <option value="">* PPUBNO *</option>
                        {% for item in related.PPUBNO %}
                            <option value="{{ item.REFNO }}">{{ item.PPUBNO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PPUBCOL'><div>Pod pubescence color:</div><div>
                    <select>
                        <option value="">* PPUBCOL *</option>
                        {% for item in related.PPUBCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PPUBCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PPUBLEN'><div>Pod pubescence length:</div><div>
                    <select>
                        <option value="">* PPUBLEN *</option>
                        {% for item in related.PPUBLEN %}
                            <option value="{{ item.REFNO }}">{{ item.PPUBLEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PODLEN'><div>Pod length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PODLEN" /></div></div>
            <div data-type='SDPPOD'><div>Seeds per pods:</div><div><input type="number" step="0.01" min="0" placeholder="SDPPOD" /></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='SDCTCOL'><div>Seed coat color:</div><div>
                    <select>
                        <option value="">* SDCTCOL *</option>
                        {% for item in related.SDCTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LUSTRE'><div>Lustre on seed surface:</div><div>
                    <select>
                        <option value="">* LUSTRE *</option>
                        {% for item in related.LUSTRE %}
                            <option value="{{ item.REFNO }}">{{ item.LUSTRE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HILUM'><div>Seed hilum color:</div><div>
                    <select>
                        <option value="">* HILUM *</option>
                        {% for item in related.HILUM %}
                            <option value="{{ item.REFNO }}">{{ item.HILUM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>1000 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->