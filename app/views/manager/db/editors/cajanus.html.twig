<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-2"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-3"><span>FRUIT DATA</span></a></li>
            <li><a href="#section-line-4"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-5"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='GHBT'><div>Growth habit:</div><div>
                    <select>
                        <option value="">* GHBT *</option>
                        {% for item in related.GHBT %}
                            <option value="{{ item.REFNO }}">{{ item.GHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STCOL'><div>Stem color:</div><div>
                    <select>
                        <option value="">* STCOL *</option>
                        {% for item in related.STCOL %}
                            <option value="{{ item.REFNO }}">{{ item.STCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFLEN'><div>Leaf length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFLEN" /></div></div>
            <div data-type='LFWID'><div>Leaf width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFWID" /></div></div>
            <div data-type='LENWID'><div>L/W:</div><div><input type="number" step="0.01" min="0" placeholder="LENWID" /></div></div>
            <div data-type='LFPUBAD'><div>Leaf pubescence (adaxial):</div><div>
                    <select>
                        <option value="">* LFPUBAD *</option>
                        {% for item in related.LFPUBAD %}
                            <option value="{{ item.REFNO }}">{{ item.LFPUBAD }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFPUBAB'><div>Leaf pubescence (abaxial):</div><div>
                    <select>
                        <option value="">* LFPUBAB *</option>
                        {% for item in related.LFPUBAB %}
                            <option value="{{ item.REFNO }}">{{ item.LFPUBAB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LEATEND'><div>Leaves tenderness:</div><div>
                    <select>
                        <option value="">* LEATEND *</option>
                        {% for item in related.LEATEND %}
                            <option value="{{ item.REFNO }}">{{ item.LEATEND }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='FLCOL'><div>Flower color:</div><div>
                    <select>
                        <option value="">* FLCOL *</option>
                        {% for item in related.FLCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PATT'><div>Pattern of streaks:</div><div>
                    <select>
                        <option value="">* PATT *</option>
                        {% for item in related.PATT %}
                            <option value="{{ item.REFNO }}">{{ item.PATT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='RACNO'><div>Raceme number:</div><div><input type="number" step="0.01" min="0" placeholder="RACNO" /></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='PDCOL'><div>Pod color:</div><div>
                    <select>
                        <option value="">* PDCOL *</option>
                        {% for item in related.PDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDLEN'><div>Pod length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDLEN" /></div></div>
            <div data-type='PDWID'><div>Pod width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDWID" /></div></div>
            <div data-type='PDPUBDEN'><div>Pod pubescence density:</div><div>
                    <select>
                        <option value="">* PDPUBDEN *</option>
                        {% for item in related.PDPUBDEN %}
                            <option value="{{ item.REFNO }}">{{ item.PDPUBDEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TENDER'><div>Tenderness of young pods:</div><div>
                    <select>
                        <option value="">* TENDER *</option>
                        {% for item in related.TENDER %}
                            <option value="{{ item.REFNO }}">{{ item.TENDER }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOSDSPD'><div>Number of seeds per pod:</div><div><input type="number" step="0.01" min="0" placeholder="NOSDSPD" /></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='SDCOLP'><div>Seed color pattern:</div><div>
                    <select>
                        <option value="">* SDCOLP *</option>
                        {% for item in related.SDCOLP %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOLP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDMCOL'><div>Seed main color:</div><div>
                    <select>
                        <option value="">* SDMCOL *</option>
                        {% for item in related.SDMCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDMCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSCOL'><div>Seed second color:</div><div>
                    <select>
                        <option value="">* SDSCOL *</option>
                        {% for item in related.SDSCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDSCOL }}</option>
                        {% endfor %}
                    </select></div></div>         
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SDSHP *</option>
                        {% for item in related.SDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.SDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDLEN'><div>Seed length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDLEN" /></div></div>
            <div data-type='SDWID'><div>Seed width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWID" /></div></div>         
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-5">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->