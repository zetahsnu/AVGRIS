<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='YRSN'><div>Char. Year & Season:</div><div><input type="text" placeholder="YRSN" /></div></div>
    <div data-type='DATE1'><div>Date evaluated 1 (AVRDC-GRSU):</div><div><input type="text" placeholder="DATE1" /></div></div>
    <div data-type='DATE2'><div>Date evaluated 2 (other AVRDC units):</div><div><input type="text" placeholder="DATE2" /></div></div>
    <div data-type='DATE3'><div>Date evaluated 3 (none-AVRDC):</div><div><input type="text" placeholder="DATE3" /></div></div>
    <div data-type='PROVIDER'><div>Provider of source of information:</div><div><input type="text" placeholder="PROVIDER" /></div></div>
    <div data-type='INS_PROV'><div>Institute of provider:</div><div><input type="text" placeholder="INS_PROV" /></div></div>
    <div data-type='CTY_PROV'><div>Country of provider:</div><div><input type="text" placeholder="CTY_PROV" /></div></div>
    <div data-type='NOTES'><div>Notes:</div><div><input type="text" placeholder="NOTES" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh5">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>NUTRITIVE VALUE</span></a></li>
            <li><a href="#section-line-2"><span>PEST & DISEASE RESISTANCE</span></a></li>
            <li><a href="#section-line-3"><span>ABIOTIC STRESS TOLERANCE</span></a></li>
            <li><a href="#section-line-4"><span>other</span></a></li>
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='DRYMAT'><div>Dry matter (%):</div><div><input type="number" step="0.01" min="0" placeholder="DRYMAT" /></div></div>
            <div data-type='SUGAR'><div>Sugar (%):</div><div><input type="number" step="0.01" min="0" placeholder="SUGAR" /></div></div>
