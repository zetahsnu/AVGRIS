<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>FRUIT & SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-5"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='ANTHO'><div>Anthocyanin coloration of hypocotyl:</div><div>
                    <select>
                        <option value="">* ANTHO *</option>
                        {% for item in related.ANTHO %}
                            <option value="{{ item.REFNO }}">{{ item.ANTHO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HCOLINT'><div>Hypocotyl color intensity:</div><div>
                    <select>
                        <option value="">* HCOLINT *</option>
                        {% for item in related.HCOLINT %}
                            <option value="{{ item.REFNO }}">{{ item.HCOLINT }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='PHT'><div>Plant height (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PHT" /></div></div>
            <div data-type='PTYP'><div>Plant type:</div><div>
                    <select>
                        <option value="">* PTYP *</option>
                        {% for item in related.PTYP %}
                            <option value="{{ item.REFNO }}">{{ item.PTYP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STCOL'><div>Stem color:</div><div>
                    <select>
                        <option value="">* STCOL *</option>
                        {% for item in related.STCOL %}
                            <option value="{{ item.REFNO }}">{{ item.STCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BHBT'><div>Branching habit:</div><div>
                    <select>
                        <option value="">* BHBT *</option>
                        {% for item in related.BHBT %}
                            <option value="{{ item.REFNO }}">{{ item.BHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFBLOB'><div>Leaf blade lobing:</div><div>
                    <select>
                        <option value="">* LFBLOB *</option>
                        {% for item in related.LFBLOB %}
                            <option value="{{ item.REFNO }}">{{ item.LFBLOB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LEFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LEFCOL *</option>
                        {% for item in related.LEFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LEFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TLFTSHP'><div>Terminal leaflet shape:</div><div>
                    <select>
                        <option value="">* TLFTSHP *</option>
                        {% for item in related.TLFTSHP %}
                            <option value="{{ item.REFNO }}">{{ item.TLFTSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LEFMAR'><div>Leaf shape of margin:</div><div>
                    <select>
                        <option value="">* LEFMAR *</option>
                        {% for item in related.LEFMAR %}
                            <option value="{{ item.REFNO }}">{{ item.LEFMAR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LEFLEN'><div>Leaflet length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LEFLEN" /></div></div>
            <div data-type='LEFWD'><div>Leaflet width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LEFWD" /></div></div>
            <div data-type='PETLEN'><div>Petiole length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PETLEN" /></div></div>
            <div data-type='LFPUB'><div>Leaf pubescence:</div><div>
                    <select>
                        <option value="">* LFPUB *</option>
                        {% for item in related.LFPUB %}
                            <option value="{{ item.REFNO }}">{{ item.LFPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COLPET'><div>Color of petiole:</div><div>
                    <select>
                        <option value="">* COLPET *</option>
                        {% for item in related.COLPET %}
                            <option value="{{ item.REFNO }}">{{ item.COLPET }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFLO'><div>Days to flower:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='LENFILA'><div>Length of filament (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENFILA" /></div></div>
            <div data-type='FLOCOL'><div>Flower color:</div><div>
                    <select>
                        <option value="">* FLOCOL *</option>
                        {% for item in related.FLOCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLOCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='ANDR'><div>Androphore:</div><div>
                    <select>
                        <option value="">* ANDR *</option>
                        {% for item in related.ANDR %}
                            <option value="{{ item.REFNO }}">{{ item.ANDR }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='DFRUIT'><div>Days to fruit:</div><div><input type="number" step="1" min="0" placeholder="DFRUIT" /></div></div>
            <div data-type='PDLEN'><div>Pod length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="PDLEN" /></div></div>
            <div data-type='PDWID'><div>Pod width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="PDWID" /></div></div>
            <div data-type='LENGYNO'><div>Length of gynophore (mm):</div><div><input type="number" step="0.01" min="0" placeholder="LENGYNO" /></div></div>
            <div data-type='LENANDR'><div>Length of androphore (mm):</div><div><input type="number" step="0.01" min="0" placeholder="LENANDR" /></div></div>
            <div data-type='LENPED'><div>Length of pedicel (mm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPED" /></div></div>
            <div data-type='IMPDCOL'><div>Immature pod color:</div><div>
                    <select>
                        <option value="">* IMPDCOL *</option>
                        {% for item in related.IMPDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.IMPDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDSHP'><div>Pod shape:</div><div>
                    <select>
                        <option value="">* PDSHP *</option>
                        {% for item in related.PDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.PDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>1000 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-5">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->