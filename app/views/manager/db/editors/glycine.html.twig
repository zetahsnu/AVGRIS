<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>SEED DATA</span></a></li>
            <li><a href="#section-line-5"><span>other</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='HCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HCOL *</option>
                        {% for item in related.HCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='STDET'><div>Stem determination:</div><div>
                    <select>
                        <option value="">* STDET *</option>
                        {% for item in related.STDET %}
                            <option value="{{ item.REFNO }}">{{ item.STDET }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOLEAF'><div>Number of leaflets:</div><div>
                    <select>
                        <option value="">* NOLEAF *</option>
                        {% for item in related.NOLEAF %}
                            <option value="{{ item.REFNO }}">{{ item.NOLEAF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFSIZE'><div>Leaflet size:</div><div>
                    <select>
                        <option value="">* LFSIZE *</option>
                        {% for item in related.LFSIZE %}
                            <option value="{{ item.REFNO }}">{{ item.LFSIZE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFSHP'><div>Leaflet shape:</div><div>
                    <select>
                        <option value="">* LFSHP *</option>
                        {% for item in related.LFSHP %}
                            <option value="{{ item.REFNO }}">{{ item.LFSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PUBDEN'><div>Pubescence density:</div><div>
                    <select>
                        <option value="">* PUBDEN *</option>
                        {% for item in related.PUBDEN %}
                            <option value="{{ item.REFNO }}">{{ item.PUBDEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PUBCOL'><div>Pubescence color:</div><div>
                    <select>
                        <option value="">* PUBCOL *</option>
                        {% for item in related.PUBCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PUBCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PUBTYP'><div>Pubescence type:</div><div>
                    <select>
                        <option value="">* PUBTYP *</option>
                        {% for item in related.PUBTYP %}
                            <option value="{{ item.REFNO }}">{{ item.PUBTYP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLHTR1'><div>Plant height at R1:</div><div><input type="number" step="0.01" min="0" placeholder="PLHTR1" /></div></div>
            <div data-type='PLHTR8'><div>Plant height at R8:</div><div><input type="number" step="0.01" min="0" placeholder="PLHTR8" /></div></div>
            <div data-type='NOPBRN'><div>Number of primary branches (when at least 2 nodes) per plant at maturity:</div><div><input type="number" step="0.01" min="0" placeholder="NOPBRN" /></div></div>
            <div data-type='LODGE'><div>Lodging score:</div><div>
                    <select>
                        <option value="">* LODGE *</option>
                        {% for item in related.LODGE %}
                            <option value="{{ item.REFNO }}">{{ item.LODGE }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFLO'><div>Days to flowering:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='CORCOL'><div>Corolla color:</div><div>
                    <select>
                        <option value="">* CORCOL *</option>
                        {% for item in related.CORCOL %}
                            <option value="{{ item.REFNO }}">{{ item.CORCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='MATURE'><div>Mature pod color:</div><div>
                    <select>
                        <option value="">* MATURE *</option>
                        {% for item in related.MATURE %}
                            <option value="{{ item.REFNO }}">{{ item.MATURE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOPODS'><div>Number of pods per plant (mean of 20 randomly selected plants):</div><div><input type="number" step="0.01" min="0" placeholder="NOPODS" /></div></div>
            <div data-type='SDNO'><div>Number of seeds per pod (mean of 20 randomly selected pods):</div><div><input type="number" step="0.01" min="0" placeholder="SDNO" /></div></div>
            <div data-type='SHAT'><div>Shattering score:</div><div>
                    <select>
                        <option value="">* SHAT *</option>
                        {% for item in related.SHAT %}
                            <option value="{{ item.REFNO }}">{{ item.SHAT }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>  
        <section id="section-line-4">
            <div data-type='HRDSD'><div>Hard seeds (actual percent):</div><div><input type="number" step="0.01" min="0" placeholder="HRDSD" /></div></div>
            <div data-type='SDCOL'><div>Seed color:</div><div>
                    <select>
                        <option value="">* SDCOL *</option>
                        {% for item in related.SDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDCOAT'><div>Seed coat pattern:</div><div>
                    <select>
                        <option value="">* SDCOAT *</option>
                        {% for item in related.SDCOAT %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOAT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HILUM'><div>Hilum color:</div><div>
                    <select>
                        <option value="">* HILUM *</option>
                        {% for item in related.HILUM %}
                            <option value="{{ item.REFNO }}">{{ item.HILUM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDCSL'><div>Seed coat surface luster:</div><div>
                    <select>
                        <option value="">* SDCSL *</option>
                        {% for item in related.SDCSL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCSL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
            <div data-type='COTCOL'><div>Cotyledon color:</div><div>
                    <select>
                        <option value="">* COTCOL *</option>
                        {% for item in related.COTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.COTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TOTOIL'><div>Total oil content (percent on dry seed weight basis):</div><div><input type="number" step="0.01" min="0" placeholder="TOTOIL" /></div></div>
            <div data-type='PROTEIN'><div>Protein content (6.25 x N) (percent on dry seed weight basis):</div><div><input type="number" step="0.01" min="0" placeholder="PROTEIN" /></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='REMARK'><div>Remark:</div><div><input type="text" placeholder="REMARK" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->