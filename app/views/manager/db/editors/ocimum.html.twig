<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>FRUIT & SEED DATA</span></a></li>
            <li><a href="#section-line-5"><span>other</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='HYPCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HYPCOLIN'><div>Hypocotyl color intensity:</div><div>
                    <select>
                        <option value="">* HYPCOLIN *</option>
                        {% for item in related.HYPCOLIN %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOLIN }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='STCOL'><div>Stem color:</div><div>
                    <select>
                        <option value="">* STCOL *</option>
                        {% for item in related.STCOL %}
                            <option value="{{ item.REFNO }}">{{ item.STCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLTYP'><div>Plant type:</div><div>
                    <select>
                        <option value="">* PLTYP *</option>
                        {% for item in related.PLTYP %}
                            <option value="{{ item.REFNO }}">{{ item.PLTYP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLHT'><div>Plant height (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLHT" /></div></div>
            <div data-type='GRWRT'><div>Growth rate:</div><div>
                    <select>
                        <option value="">* GRWRT *</option>
                        {% for item in related.GRWRT %}
                            <option value="{{ item.REFNO }}">{{ item.GRWRT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFSHP'><div>Leaf shape:</div><div>
                    <select>
                        <option value="">* LFSHP *</option>
                        {% for item in related.LFSHP %}
                            <option value="{{ item.REFNO }}">{{ item.LFSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='MESOPHYL'><div>Mesophyll:</div><div>
                    <select>
                        <option value="">* MESOPHYL *</option>
                        {% for item in related.MESOPHYL %}
                            <option value="{{ item.REFNO }}">{{ item.MESOPHYL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFSURF'><div>Leaf surface:</div><div>
                    <select>
                        <option value="">* LFSURF *</option>
                        {% for item in related.LFSURF %}
                            <option value="{{ item.REFNO }}">{{ item.LFSURF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFAPX'><div>Leaf apex:</div><div>
                    <select>
                        <option value="">* LFAPX *</option>
                        {% for item in related.LFAPX %}
                            <option value="{{ item.REFNO }}">{{ item.LFAPX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFMGN'><div>Leaf margin:</div><div>
                    <select>
                        <option value="">* LFMGN *</option>
                        {% for item in related.LFMGN %}
                            <option value="{{ item.REFNO }}">{{ item.LFMGN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='VELVT'><div>Velvets:</div><div>
                    <select>
                        <option value="">* VELVT *</option>
                        {% for item in related.VELVT %}
                            <option value="{{ item.REFNO }}">{{ item.VELVT }}</option>
                        {% endfor %}
                    </select></div>
                <div data-type='VELVTCOL'><div>Velvet color:</div><div>
                        <select>
                            <option value="">* VELVTCOL *</option>
                            {% for item in related.VELVTCOL %}
                                <option value="{{ item.REFNO }}">{{ item.VELVTCOL }}</option>
                            {% endfor %}
                        </select></div></div>
                <div data-type='LFLEN'><div>Leaf length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFLEN" /></div></div>
                <div data-type='LFWID'><div>Leaf width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFWID" /></div></div>
                <div data-type='PETLEN'><div>Petiole length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="PETLEN" /></div></div>
                <div data-type='PETTHIC'><div>Petiole thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="PETTHIC" /></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFLO'><div>Days to flower:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='FLOCOL'><div>Flower color:</div><div>
                    <select>
                        <option value="">* FLOCOL *</option>
                        {% for item in related.FLOCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLOCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='DAYSFRU'><div>Days to fruit:</div><div><input type="number" step="1" min="0" placeholder="DAYSFRU" /></div></div>
            <div data-type='SDCOL'><div>Seed color:</div><div>
                    <select>
                        <option value="">* SDCOL *</option>
                        {% for item in related.SDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SDSHP *</option>
                        {% for item in related.SDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.SDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSURF'><div>Seed surface:</div><div>
                    <select>
                        <option value="">* SDSURF *</option>
                        {% for item in related.SDSURF %}
                            <option value="{{ item.REFNO }}">{{ item.SDSURF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>1000 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='REMARKS'><div>Remarks:</div><div><input type="text" placeholder="REMARKS" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->