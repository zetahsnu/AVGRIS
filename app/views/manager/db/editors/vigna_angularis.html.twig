<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>            
            <li><a href="#section-line-1"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-2"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-3"><span>POD DATA</span></a></li>
            <li><a href="#section-line-4"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-5"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GRHBT'><div>Growth habit:</div><div>
                    <select>
                        <option value="">* GRHBT *</option>
                        {% for item in related.GRHBT %}
                            <option value="{{ item.REFNO }}">{{ item.GRHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='GRPATT'><div>Growth pattern:</div><div>
                    <select>
                        <option value="">* GRPATT *</option>
                        {% for item in related.GRPATT %}
                            <option value="{{ item.REFNO }}">{{ item.GRPATT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HYPCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PRLFSHP'><div>Primary leaf shape:</div><div>
                    <select>
                        <option value="">* PRLFSHP *</option>
                        {% for item in related.PRLFSHP %}
                            <option value="{{ item.REFNO }}">{{ item.PRLFSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TLFLSHP'><div>Terminal leaflet shape:</div><div>
                    <select>
                        <option value="">* TLFLSHP *</option>
                        {% for item in related.TLFLSHP %}
                            <option value="{{ item.REFNO }}">{{ item.TLFLSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TLFLLEN'><div>Terminal leaflet length:</div><div>
                    <select>
                        <option value="">* TLFLLEN *</option>
                        {% for item in related.TLFLLEN %}
                            <option value="{{ item.REFNO }}">{{ item.TLFLLEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PUBDEN'><div>Pubescence density:</div><div>
                    <select>
                        <option value="">* PUBDEN *</option>
                        {% for item in related.PUBDEN %}
                            <option value="{{ item.REFNO }}">{{ item.PUBDEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFPUBLEN'><div>Leaf pubescence length:</div><div>
                    <select>
                        <option value="">* LFPUBLEN *</option>
                        {% for item in related.LFPUBLEN %}
                            <option value="{{ item.REFNO }}">{{ item.LFPUBLEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PETCOL'><div>Petiole color:</div><div>
                    <select>
                        <option value="">* PETCOL *</option>
                        {% for item in related.PETCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PETCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COLBPET'><div>Color of basal petioles:</div><div>
                    <select>
                        <option value="">* COLBPET *</option>
                        {% for item in related.COLBPET %}
                            <option value="{{ item.REFNO }}">{{ item.COLBPET }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='RACPOS'><div>Raceme position:</div><div>
                    <select>
                        <option value="">* RACPOS *</option>
                        {% for item in related.RACPOS %}
                            <option value="{{ item.REFNO }}">{{ item.RACPOS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='CALCOL'><div>Calyx color:</div><div>
                    <select>
                        <option value="">* CALCOL *</option>
                        {% for item in related.CALCOL %}
                            <option value="{{ item.REFNO }}">{{ item.CALCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SPETLCOL'><div>Standard petal color:</div><div>
                    <select>
                        <option value="">* SPETLCOL *</option>
                        {% for item in related.SPETLCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SPETLCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='WPETLCOL'><div>Wings petal color:</div><div>
                    <select>
                        <option value="">* WPETLCOL *</option>
                        {% for item in related.WPETLCOL %}
                            <option value="{{ item.REFNO }}">{{ item.WPETLCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='DAYSFLO'><div>Days to 50% flowering:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='PLHTFLO'><div>Plant height at flowering (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLHTFLO" /></div></div>
            <div data-type='PLHTMAT'><div>Plant height at maturation (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLHTMAT" /></div></div>
            <div data-type='SDSPPOD'><div>Seeds per pods (means of 10 pods):</div><div><input type="number" step="0.01" min="0" placeholder="SDSPPOD" /></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='PDCOLIMS'><div>Pod color at immature stage:</div><div>
                    <select>
                        <option value="">* PDCOLIMS *</option>
                        {% for item in related.PDCOLIMS %}
                            <option value="{{ item.REFNO }}">{{ item.PDCOLIMS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDCOLMS'><div>Pod color at mature stage:</div><div>
                    <select>
                        <option value="">* PDCOLMS *</option>
                        {% for item in related.PDCOLMS %}
                            <option value="{{ item.REFNO }}">{{ item.PDCOLMS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDLEN'><div>Pod length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDLEN" /></div></div>
            <div data-type='DAYSPMAT'><div>Days to 50% pod mature:</div><div><input type="number" step="1" min="0" placeholder="DAYSPMAT" /></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='MOTSD'><div>Mottling on seed:</div><div>
                    <select>
                        <option value="">* MOTSD *</option>
                        {% for item in related.MOTSD %}
                            <option value="{{ item.REFNO }}">{{ item.MOTSD }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LUSDSUR'><div>Lustre on seed surface:</div><div>
                    <select>
                        <option value="">* LUSDSUR *</option>
                        {% for item in related.LUSDSUR %}
                            <option value="{{ item.REFNO }}">{{ item.LUSDSUR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDCTCOL'><div>Seed coat color:</div><div>
                    <select>
                        <option value="">* SDCTCOL *</option>
                        {% for item in related.SDCTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-5">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->