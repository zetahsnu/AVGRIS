<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>            
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>FRUIT DATA</span></a></li>
            <li><a href="#section-line-5"><span>SEED DATA</span></a></li>
            <li><a href="#section-line-6"><span>other</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-7"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='COLELE'><div>Cotyledonous leaf length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="COLELE" /></div></div>
            <div data-type='COLEWI'><div>Cotyledonous leaf width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="COLEWI" /></div></div>
            <div data-type='COCOL'><div>Cotyledonous leaf color:</div><div>
                    <select>
                        <option value="">* COCOL *</option>
                        {% for item in related.COCOL %}
                            <option value="{{ item.REFNO }}">{{ item.COCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COLWR'><div>Cotyledon length/width ratio:</div><div>
                    <select>
                        <option value="">* COLWR *</option>
                        {% for item in related.COLWR %}
                            <option value="{{ item.REFNO }}">{{ item.COLWR }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='PGHBT'><div>Plant growth habit:</div><div>
                    <select>
                        <option value="">* PGHBT *</option>
                        {% for item in related.PGHBT %}
                            <option value="{{ item.REFNO }}">{{ item.PGHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STRIDG'><div>Stem ridging:</div><div>
                    <select>
                        <option value="">* STRIDG *</option>
                        {% for item in related.STRIDG %}
                            <option value="{{ item.REFNO }}">{{ item.STRIDG }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SPINST'><div>Spines on stem:</div><div>
                    <select>
                        <option value="">* SPINST *</option>
                        {% for item in related.SPINST %}
                            <option value="{{ item.REFNO }}">{{ item.SPINST }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='DEGSTPUB'><div>Degree of stem pubescence:</div><div>
                    <select>
                        <option value="">* DEGSTPUB *</option>
                        {% for item in related.DEGSTPUB %}
                            <option value="{{ item.REFNO }}">{{ item.DEGSTPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PHT'><div>Plant height (cm) (at flowering stage):</div><div><input type="number" step="0.01" min="0" placeholder="PHT" /></div></div>
            <div data-type='PBRD'><div>Plant breadth (cm) (at flowering stage):</div><div><input type="number" step="0.01" min="0" placeholder="PBRD" /></div></div>
            <div data-type='PBRNCH'><div>Plant branching (no. of primary branches per plant):</div><div><input type="number" step="0.01" min="0" placeholder="PBRNCH" /></div></div>
            <div data-type='PETCOL'><div>Petiole color:</div><div>
                    <select>
                        <option value="">* PETCOL *</option>
                        {% for item in related.PETCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PETCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PETLEN'><div>Petiole length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="PETLEN" /></div></div>
            <div data-type='LFBLLEN'><div>Leaf blade length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFBLLEN" /></div></div>
            <div data-type='LFBLWID'><div>Leaf blade width (cm) (maximum width):</div><div><input type="number" step="0.01" min="0" placeholder="LFBLWID" /></div></div>
            <div data-type='LFBLLOB'><div>Leaf blade lobes:</div><div>
                    <select>
                        <option value="">* LFBLLOB *</option>
                        {% for item in related.LFBLLOB %}
                            <option value="{{ item.REFNO }}">{{ item.LFBLLOB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFBLTA'><div>Leaf blade tip angle:</div><div>
                    <select>
                        <option value="">* LFBLTA *</option>
                        {% for item in related.LFBLTA %}
                            <option value="{{ item.REFNO }}">{{ item.LFBLTA }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFBLCOL'><div>Leaf blade color (upper surface):</div><div>
                    <select>
                        <option value="">* LFBLCOL *</option>
                        {% for item in related.LFBLCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFBLCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFPRCK'><div>Leaf prickles (no. of leaf prickles on upper surface of the leaf):</div><div><input type="number" step="0.01" min="0" placeholder="LFPRCK" /></div></div>
            <div data-type='LFHAIRS'><div>Leaf hairs (no. of hair per mm2 on lower surface of the leaf):</div><div>
                    <select>
                        <option value="">* LFHAIRS *</option>
                        {% for item in related.LFHAIRS %}
                            <option value="{{ item.REFNO }}">{{ item.LFHAIRS }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='NOFLWRPINF'><div>Number of flowers per inflorescence:</div><div><input type="number" step="0.01" min="0" placeholder="NOFLWRPINF" /></div></div>
            <div data-type='FLOTME'><div>Flowering time (no. of days from sowing until first flower opening):</div><div><input type="number" step="1" min="0" placeholder="FLOTME" /></div></div>
            <div data-type='STALEN'><div>Stamen length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="STALEN" /></div></div>
            <div data-type='PELEN'><div>Petal length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PELEN" /></div></div>
            <div data-type='SEPLEN'><div>Sepal length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="SEPLEN" /></div></div>
            <div data-type='NOHER'><div>Number of hermaphrodite flowers per inflorescence:</div><div>
                    <select>
                        <option value="">* NOHER *</option>
                        {% for item in related.NOHER %}
                            <option value="{{ item.REFNO }}">{{ item.NOHER }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='CORCOL'><div>Corolla color:</div><div>
                    <select>
                        <option value="">* CORCOL *</option>
                        {% for item in related.CORCOL %}
                            <option value="{{ item.REFNO }}">{{ item.CORCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='RELSTY'><div>Relative style length (mm):</div><div>
                    <select>
                        <option value="">* RELSTY *</option>
                        {% for item in related.RELSTY %}
                            <option value="{{ item.REFNO }}">{{ item.RELSTY }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='POLPROD'><div>Pollen production:</div><div>
                    <select>
                        <option value="">* POLPROD *</option>
                        {% for item in related.POLPROD %}
                            <option value="{{ item.REFNO }}">{{ item.POLPROD }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STYLE'><div>Style Exsertion:</div><div>
                    <select>
                        <option value="">* STYLE *</option>
                        {% for item in related.STYLE %}
                            <option value="{{ item.REFNO }}">{{ item.STYLE }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='FRLENG'><div>Fruit length (in cm; from base of calyx to tip of fruit):</div><div><input type="number" step="0.01" min="0" placeholder="FRLENG" /></div></div>
            <div data-type='FRBREAD'><div>Fruit breadth (in cm; diameter at broadest part):</div><div><input type="number" step="0.01" min="0" placeholder="FRBREAD" /></div></div>
            <div data-type='FRLBRTO'><div>Fruit length/breadth ratio:</div><div>
                    <select>
                        <option value="">* FRLBRTO *</option>
                        {% for item in related.FRLBRTO %}
                            <option value="{{ item.REFNO }}">{{ item.FRLBRTO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRCURV'><div>Fruit curvature:</div><div>
                    <select>
                        <option value="">* FRCURV *</option>
                        {% for item in related.FRCURV %}
                            <option value="{{ item.REFNO }}">{{ item.FRCURV }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRPEDLEN'><div>Fruit pedicel length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="FRPEDLEN" /></div></div>
            <div data-type='FRPEDTHICK'><div>Fruit pedicel thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="FRPEDTHICK" /></div></div>
            <div data-type='FRPEDPRICK'><div>Fruit pedicel prickles:</div><div>
                    <select>
                        <option value="">* FRPEDPRICK *</option>
                        {% for item in related.FRPEDPRICK %}
                            <option value="{{ item.REFNO }}">{{ item.FRPEDPRICK }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSHP'><div>Fruit shape:</div><div>
                    <select>
                        <option value="">* FRSHP *</option>
                        {% for item in related.FRSHP %}
                            <option value="{{ item.REFNO }}">{{ item.FRSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRAPEX'><div>Fruit apex shape:</div><div>
                    <select>
                        <option value="">* FRAPEX *</option>
                        {% for item in related.FRAPEX %}
                            <option value="{{ item.REFNO }}">{{ item.FRAPEX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRCOL_CR'><div>Fruit color at commercial ripeness:</div><div>
                    <select>
                        <option value="">* FRCOL_CR *</option>
                        {% for item in related.FRCOL_CR %}
                            <option value="{{ item.REFNO }}">{{ item.FRCOL_CR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRCOLDIST_CR'><div>Fruit color distribution at commercial ripeness:</div><div>
                    <select>
                        <option value="">* FRCOLDIST_CR *</option>
                        {% for item in related.FRCOLDIST_CR %}
                            <option value="{{ item.REFNO }}">{{ item.FRCOLDIST_CR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRCOL_PR'><div>Fruit color at physiological ripeness:</div><div>
                    <select>
                        <option value="">* FRCOL_PR *</option>
                        {% for item in related.FRCOL_PR %}
                            <option value="{{ item.REFNO }}">{{ item.FRCOL_PR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRPOS'><div>Fruit position:</div><div>
                    <select>
                        <option value="">* FRPOS *</option>
                        {% for item in related.FRPOS %}
                            <option value="{{ item.REFNO }}">{{ item.FRPOS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='RELFRCALYX'><div>Relative fruit calyx length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="RELFRCALYX" /></div></div>
            <div data-type='FRCALPRICK'><div>Fruit calyx prickles:</div><div><input type="number" step="0.01" min="0" placeholder="FRCALPRICK" /></div></div>
            <div data-type='FRCROSS'><div>Fruit cross section:</div><div>
                    <select>
                        <option value="">* FRCROSS *</option>
                        {% for item in related.FRCROSS %}
                            <option value="{{ item.REFNO }}">{{ item.FRCROSS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOLOCU'><div>Number of locules per fruit:</div><div><input type="number" step="0.01" min="0" placeholder="NOLOCU" /></div></div>
            <div data-type='FRFLDEN'><div>Fruit flesh density:</div><div>
                    <select>
                        <option value="">* FRFLDEN *</option>
                        {% for item in related.FRFLDEN %}
                            <option value="{{ item.REFNO }}">{{ item.FRFLDEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOFRPINF'><div>Number of fruit per infructescence:</div><div><input type="number" step="0.01" min="0" placeholder="NOFRPINF" /></div></div>
            <div data-type='NOFRPPL'><div>Number of fruit per plant:</div><div><input type="number" step="0.01" min="0" placeholder="NOFRPPL" /></div></div>
            <div data-type='FRYLD'><div>Fruit yield per plant (gm):</div><div>
                    <select>
                        <option value="">* FRYLD *</option>
                        {% for item in related.FRYLD %}
                            <option value="{{ item.REFNO }}">{{ item.FRYLD }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRFLAV'><div>Fruit flavor:</div><div>
                    <select>
                        <option value="">* FRFLAV *</option>
                        {% for item in related.FRFLAV %}
                            <option value="{{ item.REFNO }}">{{ item.FRFLAV }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='VAMICO'><div>Varietal mixture condition:</div><div>
                    <select>
                        <option value="">* VAMICO *</option>
                        {% for item in related.VAMICO %}
                            <option value="{{ item.REFNO }}">{{ item.VAMICO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLSHBRWN'><div>Flesh browning (after cutting):</div><div>
                    <select>
                        <option value="">* FLSHBRWN *</option>
                        {% for item in related.FLSHBRWN %}
                            <option value="{{ item.REFNO }}">{{ item.FLSHBRWN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HARVEST'><div>Harvest procedure:</div><div>
                    <select>
                        <option value="">* HARVEST *</option>
                        {% for item in related.HARVEST %}
                            <option value="{{ item.REFNO }}">{{ item.HARVEST }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='SDCOL'><div>Seed color:</div><div>
                    <select>
                        <option value="">* SDCOL *</option>
                        {% for item in related.SDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOSDS'><div>Number of seeds per fruit:</div><div>
                    <select>
                        <option value="">* NOSDS *</option>
                        {% for item in related.NOSDS %}
                            <option value="{{ item.REFNO }}">{{ item.NOSDS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDDEN'><div>Seed density:</div><div>
                    <select>
                        <option value="">* SDDEN *</option>
                        {% for item in related.SDDEN %}
                            <option value="{{ item.REFNO }}">{{ item.SDDEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSZE'><div>Seed size (mm):</div><div>
                    <select>
                        <option value="">* SDSZE *</option>
                        {% for item in related.SDSZE %}
                            <option value="{{ item.REFNO }}">{{ item.SDSZE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        <section id="section-line-6">
            <div data-type='REMARKS'><div>Remarks:</div><div><input type="text" placeholder="REMARKS" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-7">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->