<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>Seeding Data</span></a></li>
            <li><a href="#section-line-2"><span>Vegetative Data</span></a></li>
            <li><a href="#section-line-3"><span>Inflorescence Data</span></a></li>
            <li><a href="#section-line-4"><span>Fruit Data</span></a></li>
            <li><a href="#section-line-5"><span>Seed Data</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='HYPCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COTCOL'><div>Cotyledon color:</div><div>
                    <select>
                        <option value="">* COTCOL *</option>
                        {% for item in related.COTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.COTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='PLGHBT'><div>Plant growth habit:</div><div>
                    <select>
                        <option value="">* PLGHBT *</option>
                        {% for item in related.PLGHBT %}
                            <option value="{{ item.REFNO }}">{{ item.PLGHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLHT'><div>Plant height (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLHT" /></div></div>
            <div data-type='NOINTNOD'><div>Number of internodes:</div><div><input type="number" step="0.01" min="0" placeholder="NOINTNOD" /></div></div>
            <div data-type='BRHBT'><div>Branching habit:</div><div>
                    <select>
                        <option value="">* BRHBT *</option>
                        {% for item in related.BRHBT %}
                            <option value="{{ item.REFNO }}">{{ item.BRHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STDIA'><div>Stem diameter measured at the base (mm):</div><div><input type="number" step="0.01" min="0" placeholder="STDIA" /></div></div>
            <div data-type='STPUB'><div>Stem pubescence:</div><div>
                    <select>
                        <option value="">* STPUB *</option>
                        {% for item in related.STPUB %}
                            <option value="{{ item.REFNO }}">{{ item.STPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STCOL'><div>Stem color:</div><div>
                    <select>
                        <option value="">* STCOL *</option>
                        {% for item in related.STCOL %}
                            <option value="{{ item.REFNO }}">{{ item.STCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFSHP'><div>Leaf shape:</div><div>
                    <select>
                        <option value="">* LFSHP *</option>
                        {% for item in related.LFSHP %}
                            <option value="{{ item.REFNO }}">{{ item.LFSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFPUB'><div>Leaf pubescence:</div><div>
                    <select>
                        <option value="">* LFPUB *</option>
                        {% for item in related.LFPUB %}
                            <option value="{{ item.REFNO }}">{{ item.LFPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PETCOL'><div>Petiole color:</div><div>
                    <select>
                        <option value="">* PETCOL *</option>
                        {% for item in related.PETCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PETCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='FLODATE'><div>Flowering date:</div><div><input type="date" placeholder="FLODATE" /></div></div>
            <div data-type='NONODFF'><div>Number of nodes to first flower:</div><div><input type="number" step="0.01" min="0" placeholder="NONODFF" /></div></div>
            <div data-type='CORCOL'><div>Corolla color:</div><div>
                    <select>
                        <option value="">* CORCOL *</option>
                        {% for item in related.CORCOL %}
                            <option value="{{ item.REFNO }}">{{ item.CORCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='RCOLBCOR'><div>Red color on the base of corolla:</div><div>
                    <select>
                        <option value="">* RCOLBCOR *</option>
                        {% for item in related.RCOLBCOR %}
                            <option value="{{ item.REFNO }}">{{ item.RCOLBCOR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOCALSEG'><div>Number of calyx segments:</div><div>
                    <select>
                        <option value="">* NOCALSEG *</option>
                        {% for item in related.NOCALSEG %}
                            <option value="{{ item.REFNO }}">{{ item.NOCALSEG }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SHPCALSEG'><div>Shape of calyx segments:</div><div>
                    <select>
                        <option value="">* SHPCALSEG *</option>
                        {% for item in related.SHPCALSEG %}
                            <option value="{{ item.REFNO }}">{{ item.SHPCALSEG }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PERCALSEG'><div>Persistence of calyx segments:</div><div>
                    <select>
                        <option value="">* PERCALSEG *</option>
                        {% for item in related.PERCALSEG %}
                            <option value="{{ item.REFNO }}">{{ item.PERCALSEG }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='POFRPSTM'><div>Position of fruits at the primary stem:</div><div>
                    <select>
                        <option value="">* POFRPSTM *</option>
                        {% for item in related.POFRPSTM %}
                            <option value="{{ item.REFNO }}">{{ item.POFRPSTM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='DATEFRMAT'><div>Date to fruit maturity:</div><div><input type="date" placeholder="DATEFRMAT" /></div></div>
            <div data-type='FRCOL'><div>Fruit color:</div><div>
                    <select>
                        <option value="">* FRCOL *</option>
                        {% for item in related.FRCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FRCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRLEN'><div>Fruit length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRLEN" /></div></div>
            <div data-type='FRPUB'><div>Fruit pubescence:</div><div>
                    <select>
                        <option value="">* FRPUB *</option>
                        {% for item in related.FRPUB %}
                            <option value="{{ item.REFNO }}">{{ item.FRPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOLOCPFR'><div>Number of locules per fruit:</div><div>
                    <select>
                        <option value="">* NOLOCPFR *</option>
                        {% for item in related.NOLOCPFR %}
                            <option value="{{ item.REFNO }}">{{ item.NOLOCPFR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRTYPE'><div>Fruit type:</div><div>
                    <select>
                        <option value="">* FRTYPE *</option>
                        {% for item in related.FRTYPE %}
                            <option value="{{ item.REFNO }}">{{ item.FRTYPE }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='NOSDSPFR'><div>Number of seeds per fruit:</div><div><input type="number" step="0.01" min="0" placeholder="NOSDSPFR" /></div></div>
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SDSHP *</option>
                        {% for item in related.SDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.SDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSURF'><div>Seed surface:</div><div>
                    <select>
                        <option value="">* SDSURF *</option>
                        {% for item in related.SDSURF %}
                            <option value="{{ item.REFNO }}">{{ item.SDSURF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>1000 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->