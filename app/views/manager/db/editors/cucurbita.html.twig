            <div data-type='FLECOLIN'><div>Flesh color intensity:</div><div>
                    <select>
                        <option value="">* FLECOLIN *</option>
                        {% for item in related.FLECOLIN %}
                            <option value="{{ item.REFNO }}">{{ item.FLECOLIN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLEMOS'><div>Flesh moisture:</div><div>
                    <select>
                        <option value="">* FLEMOS *</option>
                        {% for item in related.FLEMOS %}
                            <option value="{{ item.REFNO }}">{{ item.FLEMOS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLEFLA'><div>Flesh flavor:</div><div>
