                    <select>
                        <option value="">* DESIGNPRO *</option>
                        {% for item in related.DESIGNPRO %}
                            <option value="{{ item.REFNO }}">{{ item.DESIGNPRO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRWT'><div>Fruit weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="FRWT" /></div></div>
            <div data-type='FRLEN'><div>Fruit length at table use maturity (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRLEN" /></div></div>
            <div data-type='FRBRD'><div>Fruit breadth of fruit at table use maturity (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRBRD" /></div></div>
