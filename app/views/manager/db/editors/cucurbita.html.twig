                        {% endfor %}
                    </select></div></div>            
            <div data-type='LFMGN'><div>Leaf margin:</div><div>
                    <select>
                        <option value="">* LFMGN *</option>
                        {% for item in related.LFMGN %}
                            <option value="{{ item.REFNO }}">{{ item.LFMGN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENPET'><div>Length of petiole (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPET" /></div></div>
            <div data-type='PETSPI'><div>Petiole spines:</div><div>
                    <select>
                        <option value="">* PETSPI *</option>
                        {% for item in related.PETSPI %}
                            <option value="{{ item.REFNO }}">{{ item.PETSPI }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='INTMOTT'><div>Interveinal mottling:</div><div>
                    <select>
                        <option value="">* INTMOTT *</option>
                        {% for item in related.INTMOTT %}
                            <option value="{{ item.REFNO }}">{{ item.INTMOTT }}</option>
                        {% endfor %}
                    </select></div></div>            
            <div data-type='PLGRHBT'><div>Plant growth habit:</div><div>
                    <select>
                        <option value="">* PLGRHBT *</option>
                        {% for item in related.PLGRHBT %}
                            <option value="{{ item.REFNO }}">{{ item.PLGRHBT }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFFF'><div>Days to first female flower:</div><div><input type="number" step="1" min="0" placeholder="DAYSFFF" /></div></div>
            <div data-type='NODFFF'><div>Node number of first female flower:</div><div><input type="number" step="0.01" min="0" placeholder="NODFFF" /></div></div>
            <div data-type='TIMEFLO'><div>Time of flowering:</div><div>
