                        {% endfor %}
                    </select></div></div>
            <div data-type='PEDSEPA'><div>Peduncle separation from fruit:</div><div>
                    <select>
                        <option value="">* PEDSEPA *</option>
                        {% for item in related.PEDSEPA %}
                            <option value="{{ item.REFNO }}">{{ item.PEDSEPA }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRSURF'><div>Fruit surface:</div><div>
                    <select>
                        <option value="">* FRSURF *</option>
                        {% for item in related.FRSURF %}
                            <option value="{{ item.REFNO }}">{{ item.FRSURF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PREFSCM'><div>Predominant fruit skin color at maturity:</div><div>
                    <select>
                        <option value="">* PREFSCM *</option>
                        {% for item in related.PREFSCM %}
                            <option value="{{ item.REFNO }}">{{ item.PREFSCM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SECFSCM'><div>Secondary fruit skin color at maturity:</div><div>
                    <select>
                        <option value="">* SECFSCM *</option>
