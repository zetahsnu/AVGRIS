                        {% endfor %}
                    </select></div></div>
            <div data-type='SDMGNCOL'><div>Seed margin color:</div><div>
                    <select>
                        <option value="">* SDMGNCOL *</option>
                        {% for item in related.SDMGNCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDMGNCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->