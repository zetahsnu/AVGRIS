            <div data-type='FRTIP'><div>Fruit tip:</div><div>
                    <select>
                        <option value="">* FRTIP *</option>
                        {% for item in related.FRTIP %}
                            <option value="{{ item.REFNO }}">{{ item.FRTIP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRRIBSHP'><div>Fruit rib shape (to be observed in cross-section):</div><div>
                    <select>
                        <option value="">* FRRIBSHP *</option>
                        {% for item in related.FRRIBSHP %}
                            <option value="{{ item.REFNO }}">{{ item.FRRIBSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENPED'><div>Length of peduncle (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPED" /></div></div>
            <div data-type='PEDDIA'><div>Peduncle diameter (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PEDDIA" /></div></div>
            <div data-type='PEDSHP'><div>Peduncle shape:</div><div>
