            <div data-type='CORDIA'><div>Corolla diameter (cm):</div><div><input type="number" step="0.01" min="0" placeholder="CORDIA" /></div></div>
            <div data-type='CORTIP'><div>Corolla tip:</div><div>
                    <select>
                        <option value="">* CORTIP *</option>
                        {% for item in related.CORTIP %}
                            <option value="{{ item.REFNO }}">{{ item.CORTIP }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='DAYSHCS'><div>Days to harvesting at commercial stage:</div><div><input type="number" step="1" min="0" placeholder="DAYSHCS" /></div></div>
            <div data-type='FRSET'><div>Fruit set:</div><div>
                    <select>
                        <option value="">* FRSET *</option>
