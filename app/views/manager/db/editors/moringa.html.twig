<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>FRUIT DATA</span></a></li>
            <li><a href="#section-line-5"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='COTCOL'><div>Cotyledon color:</div><div>
                    <select>
                        <option value="">* COTCOL *</option>
                        {% for item in related.COTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.COTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HYPCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='GRHBT'><div>Growth habit:</div><div>
                    <select>
                        <option value="">* GRHBT *</option>
                        {% for item in related.GRHBT %}
                            <option value="{{ item.REFNO }}">{{ item.GRHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLHT'><div>Plant height (cm) (at flowering stage):</div><div><input type="number" step="0.01" min="0" placeholder="PLHT" /></div></div>
            <div data-type='TRILFLEN'><div>Tripinnatum leaf length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="TRILFLEN" /></div></div>
            <div data-type='TRILFWID'><div>Tripinnatum leaf width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="TRILFWID" /></div></div>
            <div data-type='LFTYP'><div>Leaf type:</div><div>
                    <select>
                        <option value="">* LFTYP *</option>
                        {% for item in related.LFTYP %}
                            <option value="{{ item.REFNO }}">{{ item.LFTYP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFARR'><div>Leaf arrangement:</div><div>
                    <select>
                        <option value="">* LFARR *</option>
                        {% for item in related.LFARR %}
                            <option value="{{ item.REFNO }}">{{ item.LFARR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFLSHP'><div>Leaflet shape:</div><div>
                    <select>
                        <option value="">* LFLSHP *</option>
                        {% for item in related.LFLSHP %}
                            <option value="{{ item.REFNO }}">{{ item.LFLSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFLARR'><div>Leaflet arrangement:</div><div>
                    <select>
                        <option value="">* LFLARR *</option>
                        {% for item in related.LFLARR %}
                            <option value="{{ item.REFNO }}">{{ item.LFLARR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOSECRA'><div>Number of secondary rachis:</div><div><input type="number" step="0.01" min="0" placeholder="NOSECRA" /></div></div>
            <div data-type='LFCOL'><div>Leaf Color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFFLA'><div>Leaf flavor:</div><div>
                    <select>
                        <option value="">* LFFLA *</option>
                        {% for item in related.LFFLA %}
                            <option value="{{ item.REFNO }}">{{ item.LFFLA }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TRLFLLEN'><div>Tripinnatum leaflet length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="TRLFLLEN" /></div></div>
            <div data-type='TRLFLWID'><div>Tripinnatum leaflet width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="TRLFLWID" /></div></div>
            <div data-type='PETPIG'><div>Petiole pigmentation:</div><div>
                    <select>
                        <option value="">* PETPIG *</option>
                        {% for item in related.PETPIG %}
                            <option value="{{ item.REFNO }}">{{ item.PETPIG }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFLO'><div>Days to flowering:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='FLCOL'><div>Flower Color:</div><div>
                    <select>
                        <option value="">* FLCOL *</option>
                        {% for item in related.FLCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLAROM'><div>Flower aromatic:</div><div>
                    <select>
                        <option value="">* FLAROM *</option>
                        {% for item in related.FLAROM %}
                            <option value="{{ item.REFNO }}">{{ item.FLAROM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOPTL'><div>No. of Petals:</div><div><input type="number" step="0.01" min="0" placeholder="NOPTL" /></div></div>
            <div data-type='PTLLEN'><div>Petal length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PTLLEN" /></div></div>
            <div data-type='PTLWID'><div>Petal width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PTLWID" /></div></div>
            <div data-type='SEPLEN'><div>Sepal length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="SEPLEN" /></div></div>
            <div data-type='SEPWID'><div>Sepal width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="SEPWID" /></div></div>
            <div data-type='NOSEP'><div>No. of Sepals:</div><div><input type="number" step="0.01" min="0" placeholder="NOSEP" /></div></div>
            <div data-type='HTPIS'><div>Height of Pistil (cm):</div><div><input type="number" step="0.01" min="0" placeholder="HTPIS" /></div></div>
            <div data-type='HTSTA'><div>Height of Stamen (cm):</div><div><input type="number" step="0.01" min="0" placeholder="HTSTA" /></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='PDCOL'><div>Pod Color:</div><div>
                    <select>
                        <option value="">* PDCOL *</option>
                        {% for item in related.PDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDLEN'><div>Pod length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDLEN" /></div></div>
            <div data-type='PDWID'><div>Pod width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDWID" /></div></div>
            <div data-type='NOSDSPD'><div>Number of seeds per pod:</div><div><input type="number" step="0.01" min="0" placeholder="NOSDSPD" /></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='SDCOL'><div>Seed Color:</div><div>
                    <select>
                        <option value="">* SDCOL *</option>
                        {% for item in related.SDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>1000 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
            <div data-type='POLFERT'><div>Pollen fertility:</div><div><input type="number" step="0.01" min="0" placeholder="POLFERT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->