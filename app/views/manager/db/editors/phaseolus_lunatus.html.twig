<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>SEED DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE & FRUIT DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-4"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='SDLEN'><div>Seed length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDLEN" /></div></div>
            <div data-type='SDWID'><div>Seed width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWID" /></div></div>
            <div data-type='SDTHK'><div>Seed thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDTHK" /></div></div>
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SDSHP *</option>
                        {% for item in related.SDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.SDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDCTM'><div>Seed coat marble:</div><div>
                    <select>
                        <option value="">* SDCTM *</option>
                        {% for item in related.SDCTM %}
                            <option value="{{ item.REFNO }}">{{ item.SDCTM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDCTST'><div>Seed coat stripe:</div><div>
                    <select>
                        <option value="">* SDCTST *</option>
                        {% for item in related.SDCTST %}
                            <option value="{{ item.REFNO }}">{{ item.SDCTST }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='GCOLSC'><div>Ground color seed coat:</div><div>
                    <select>
                        <option value="">* GCOLSC *</option>
                        {% for item in related.GCOLSC %}
                            <option value="{{ item.REFNO }}">{{ item.GCOLSC }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COLFIR'><div>Color of first layer stripe:</div><div>
                    <select>
                        <option value="">* COLFIR *</option>
                        {% for item in related.COLFIR %}
                            <option value="{{ item.REFNO }}">{{ item.COLFIR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COLSEC'><div>Color of second layer stripe:</div><div>
                    <select>
                        <option value="">* COLSEC *</option>
                        {% for item in related.COLSEC %}
                            <option value="{{ item.REFNO }}">{{ item.COLSEC }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='COTLEN'><div>Cotyledon length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="COTLEN" /></div></div>
            <div data-type='COTWID'><div>Cotyledon width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="COTWID" /></div></div>
            <div data-type='COTCOL'><div>Cotyledon color:</div><div>
                    <select>
                        <option value="">* COTCOL *</option>
                        {% for item in related.COTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.COTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HYPCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='INTER'><div>Internode length on main stem (cm):</div><div><input type="number" step="0.01" min="0" placeholder="INTER" /></div></div>
            <div data-type='PLHT'><div>Plant height (cm) (if determinate type):</div><div><input type="number" step="0.01" min="0" placeholder="PLHT" /></div></div>
            <div data-type='BRNCH'><div>Branching:</div><div>
                    <select>
                        <option value="">* BRNCH *</option>
                        {% for item in related.BRNCH %}
                            <option value="{{ item.REFNO }}">{{ item.BRNCH }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PIGMS'><div>Pigmentation on main stem (at 4-6 weeks after transplanting):</div><div>
                    <select>
                        <option value="">* PIGMS *</option>
                        {% for item in related.PIGMS %}
                            <option value="{{ item.REFNO }}">{{ item.PIGMS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFLEN'><div>Tip leaflet length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFLEN" /></div></div>
            <div data-type='LFWID'><div>Tip leaflet width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LFWID" /></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TLFTSHP'><div>Tip leaflet shape:</div><div>
                    <select>
                        <option value="">* TLFTSHP *</option>
                        {% for item in related.TLFTSHP %}
                            <option value="{{ item.REFNO }}">{{ item.TLFTSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFHDEN'><div>Leaf hairiness density:</div><div>
                    <select>
                        <option value="">* LFHDEN *</option>
                        {% for item in related.LFHDEN %}
                            <option value="{{ item.REFNO }}">{{ item.LFHDEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLTYP'><div>Plant type:</div><div>
                    <select>
                        <option value="">* PLTYP *</option>
                        {% for item in related.PLTYP %}
                            <option value="{{ item.REFNO }}">{{ item.PLTYP }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='SOWTOF'><div>From sowing to flowering (days):</div><div><input type="number" step="1" min="0" placeholder="SOWTOF" /></div></div>
            <div data-type='NODENO'><div>Node number to first inflorescence:</div><div><input type="number" step="0.01" min="0" placeholder="NODENO" /></div></div>
            <div data-type='STCOL'><div>Color of flower standard:</div><div>
                    <select>
                        <option value="">* STCOL *</option>
                        {% for item in related.STCOL %}
                            <option value="{{ item.REFNO }}">{{ item.STCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='KEELC'><div>Color of flower keel:</div><div>
                    <select>
                        <option value="">* KEELC *</option>
                        {% for item in related.KEELC %}
                            <option value="{{ item.REFNO }}">{{ item.KEELC }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='WINGC'><div>Color of flower wing:</div><div>
                    <select>
                        <option value="">* WINGC *</option>
                        {% for item in related.WINGC %}
                            <option value="{{ item.REFNO }}">{{ item.WINGC }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDNOPINF'><div>Pod number per inflorescence:</div><div><input type="number" step="0.01" min="0" placeholder="PDNOPINF" /></div></div>
            <div data-type='PDLEN'><div>Mature pod length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDLEN" /></div></div>
            <div data-type='PDWID'><div>Mature pod width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDWID" /></div></div>
            <div data-type='PDTHK'><div>Mature pod thickness (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PDTHK" /></div></div>
            <div data-type='PDCURV'><div>Mature pod curvature:</div><div>
                    <select>
                        <option value="">* PDCURV *</option>
                        {% for item in related.PDCURV %}
                            <option value="{{ item.REFNO }}">{{ item.PDCURV }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='IPDCOL'><div>Immature pod color:</div><div>
                    <select>
                        <option value="">* IPDCOL *</option>
                        {% for item in related.IPDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.IPDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PDALEN'><div>Mature pod apex length:</div><div>
                    <select>
                        <option value="">* PDALEN *</option>
                        {% for item in related.PDALEN %}
                            <option value="{{ item.REFNO }}">{{ item.PDALEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDNOP'><div>Seed number per mature pod:</div><div><input type="number" step="0.01" min="0" placeholder="SDNOP" /></div></div>
            <div data-type='DAYTM'><div>Days to maturity:</div><div><input type="number" step="1" min="0" placeholder="DAYTM" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-4">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->