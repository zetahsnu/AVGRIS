<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>            
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>FRUIT DATA</span></a></li>
            <li><a href="#section-line-5"><span>other</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='ANTHO'><div>Anthocyanin coloration of hypocotyl:</div><div>
                    <select>
                        <option value="">* ANTHO *</option>
                        {% for item in related.ANTHO %}
                            <option value="{{ item.REFNO }}">{{ item.ANTHO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STPUB'><div>Stem pubescence:</div><div>
                    <select>
                        <option value="">* STPUB *</option>
                        {% for item in related.STPUB %}
                            <option value="{{ item.REFNO }}">{{ item.STPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLFLEN'><div>Primary leaf length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLFLEN" /></div></div>
            <div data-type='PLFWID'><div>Primary leaf width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLFWID" /></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='GHBT'><div>Growth habit (when the fruits of first truss ripen):</div><div><input type="number" step="0.01" min="0" placeholder="GHBT" /></div></div>
            <div data-type='NOLEAVES'><div>Number of leaves under first inflorescence:</div><div><input type="number" step="0.01" min="0" placeholder="NOLEAVES" /></div></div>
            <div data-type='LFATT'><div>Leaf attitude:</div><div>
                    <select>
                        <option value="">* LFATT *</option>
                        {% for item in related.LFATT %}
                            <option value="{{ item.REFNO }}">{{ item.LFATT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFTYPE'><div>Leaf type:</div><div>
                    <select>
                        <option value="">* LFTYPE *</option>
                        {% for item in related.LFTYPE %}
                            <option value="{{ item.REFNO }}">{{ item.LFTYPE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='DEGLFDIS'><div>Degree of leaf dissection:</div><div>
                    <select>
                        <option value="">* DEGLFDIS *</option>
                        {% for item in related.DEGLFDIS %}
                            <option value="{{ item.REFNO }}">{{ item.DEGLFDIS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='ACLFVEIN'><div>Anthocyanin coloration of leaf veins:</div><div>
                    <select>
                        <option value="">* ACLFVEIN *</option>
                        {% for item in related.ACLFVEIN %}
                            <option value="{{ item.REFNO }}">{{ item.ACLFVEIN }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFLO'><div>Days to flower:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='NOFLOPINF'><div>Number of flower per inflorescence:</div><div><input type="number" step="0.01" min="0" placeholder="NOFLOPINF" /></div></div>
            <div data-type='INFTYPE'><div>Inflorescence type:</div><div>
                    <select>
                        <option value="">* INFTYPE *</option>
                        {% for item in related.INFTYPE %}
                            <option value="{{ item.REFNO }}">{{ item.INFTYPE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PETLEN'><div>Petal length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PETLEN" /></div></div>
            <div data-type='SEPALEN'><div>Sepal length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="SEPALEN" /></div></div>
            <div data-type='STYPE'><div>Style type:</div><div>
                    <select>
                        <option value="">* STYPE *</option>
                        {% for item in related.STYPE %}
                            <option value="{{ item.REFNO }}">{{ item.STYPE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STLEN'><div>Stamen length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="STLEN" /></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='DAYSFRU'><div>Days to fruit:</div><div><input type="number" step="1" min="0" placeholder="DAYSFRU" /></div></div>
            <div data-type='FRSZEV'><div>Fruit size variation within a plant:</div><div>
                    <select>
                        <option value="">* FRSZEV *</option>
                        {% for item in related.FRSZEV %}
                            <option value="{{ item.REFNO }}">{{ item.FRSZEV }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PREDO'><div>Predominant fruit shape (after the fruits turn color):</div><div>
                    <select>
                        <option value="">* PREDO *</option>
                        {% for item in related.PREDO %}
                            <option value="{{ item.REFNO }}">{{ item.PREDO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='EXCOLIF'><div>Exterior color of immature fruit:</div><div>
                    <select>
                        <option value="">* EXCOLIF *</option>
                        {% for item in related.EXCOLIF %}
                            <option value="{{ item.REFNO }}">{{ item.EXCOLIF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='INTGBK'><div>Intensity of greenback:</div><div>
                    <select>
                        <option value="">* INTGBK *</option>
                        {% for item in related.INTGBK %}
                            <option value="{{ item.REFNO }}">{{ item.INTGBK }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PJNTPED'><div>Presence of jointless pedicel:</div><div>
                    <select>
                        <option value="">* PJNTPED *</option>
                        {% for item in related.PJNTPED %}
                            <option value="{{ item.REFNO }}">{{ item.PJNTPED }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='RCALYX'><div>Ribbing at calyx end:</div><div>
                    <select>
                        <option value="">* RCALYX *</option>
                        {% for item in related.RCALYX %}
                            <option value="{{ item.REFNO }}">{{ item.RCALYX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FIRM'><div>Firmness:</div><div>
                    <select>
                        <option value="">* FIRM *</option>
                        {% for item in related.FIRM %}
                            <option value="{{ item.REFNO }}">{{ item.FIRM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='RADIAL'><div>Radial cracking (environmental dependent):</div><div>
                    <select>
                        <option value="">* RADIAL *</option>
                        {% for item in related.RADIAL %}
                            <option value="{{ item.REFNO }}">{{ item.RADIAL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='CONCEN'><div>Concentric cracking (environmental dependent):</div><div>
                    <select>
                        <option value="">* CONCEN *</option>
                        {% for item in related.CONCEN %}
                            <option value="{{ item.REFNO }}">{{ item.CONCEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FRFAS'><div>Fruit fasciations:</div><div>
                    <select>
                        <option value="">* FRFAS *</option>
                        {% for item in related.FRFAS %}
                            <option value="{{ item.REFNO }}">{{ item.FRFAS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOFRUPI'><div>Number of fruit set per inflorescence (the 2nd truss):</div><div><input type="number" step="0.01" min="0" placeholder="NOFRUPI" /></div></div>
            <div data-type='FRWT'><div>Fruit weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="FRWT" /></div></div>
            <div data-type='FRLEN'><div>Fruit length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRLEN" /></div></div>
            <div data-type='FRWID'><div>Fruit width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="FRWID" /></div></div>
            <div data-type='EXCOLMF'><div>Exterior color of mature fruit:</div><div>
                    <select>
                        <option value="">* EXCOLMF *</option>
                        {% for item in related.EXCOLMF %}
                            <option value="{{ item.REFNO }}">{{ item.EXCOLMF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PEDLEN'><div>Pedicel length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="PEDLEN" /></div></div>
            <div data-type='PEDAREA'><div>Pedicel area:</div><div>
                    <select>
                        <option value="">* PEDAREA *</option>
                        {% for item in related.PEDAREA %}
                            <option value="{{ item.REFNO }}">{{ item.PEDAREA }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SZEPEDS'><div>Size of pedicel scar width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SZEPEDS" /></div></div>
            <div data-type='SZECORKY'><div>Size of corky area around pedicel scar (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SZECORKY" /></div></div>
            <div data-type='SHPPS'><div>Shape of pistil scar:</div><div>
                    <select>
                        <option value="">* SHPPS *</option>
                        {% for item in related.SHPPS %}
                            <option value="{{ item.REFNO }}">{{ item.SHPPS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BLENDS'><div>Blossom end shape:</div><div>
                    <select>
                        <option value="">* BLENDS *</option>
                        {% for item in related.BLENDS %}
                            <option value="{{ item.REFNO }}">{{ item.BLENDS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BLESCON'><div>Blossom end scar condition:</div><div>
                    <select>
                        <option value="">* BLESCON *</option>
                        {% for item in related.BLESCON %}
                            <option value="{{ item.REFNO }}">{{ item.BLESCON }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='INTFCOL'><div>Interior flesh color (pericarp):</div><div>
                    <select>
                        <option value="">* INTFCOL *</option>
                        {% for item in related.INTFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.INTFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='INTFCINT'><div>Interior flesh color intensity:</div><div>
                    <select>
                        <option value="">* INTFCINT *</option>
                        {% for item in related.INTFCINT %}
                            <option value="{{ item.REFNO }}">{{ item.INTFCINT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TRANSEC'><div>Transverse section:</div><div>
                    <select>
                        <option value="">* TRANSEC *</option>
                        {% for item in related.TRANSEC %}
                            <option value="{{ item.REFNO }}">{{ item.TRANSEC }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TFRWL'><div>Thickness of fruit wall (mm):</div><div><input type="number" step="0.01" min="0" placeholder="TFRWL" /></div></div>
            <div data-type='VBCONT'><div>Vascular bundle content:</div><div>
                    <select>
                        <option value="">* VBCONT *</option>
                        {% for item in related.VBCONT %}
                            <option value="{{ item.REFNO }}">{{ item.VBCONT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COLCORE'><div>Color of core:</div><div>
                    <select>
                        <option value="">* COLCORE *</option>
                        {% for item in related.COLCORE %}
                            <option value="{{ item.REFNO }}">{{ item.COLCORE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NOLOCU'><div>Number of locules:</div><div><input type="number" step="0.01" min="0" placeholder="NOLOCU" /></div></div>
            <div data-type='THICKP'><div>Thickness of pericarp (mm):</div><div><input type="number" step="0.01" min="0" placeholder="THICKP" /></div></div>
            <div data-type='SZECOR'><div>Size of core (mm) (in cross-section):</div><div><input type="number" step="0.01" min="0" placeholder="SZECOR" /></div></div>
            <div data-type='PUFF'><div>Puffiness:</div><div>
                    <select>
                        <option value="">* PUFF *</option>
                        {% for item in related.PUFF %}
                            <option value="{{ item.REFNO }}">{{ item.PUFF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENPSCAR'><div>Length of pedicel scar (mm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPSCAR" /></div></div>
            <div data-type='SCOLRFR'><div>Skin color of ripen fruit (observe the peeled fruit skin):</div><div>
                    <select>
                        <option value="">* SCOLRFR *</option>
                        {% for item in related.SCOLRFR %}
                            <option value="{{ item.REFNO }}">{{ item.SCOLRFR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='EPEEL'><div>Easiness of peeling:</div><div>
                    <select>
                        <option value="">* EPEEL *</option>
                        {% for item in related.EPEEL %}
                            <option value="{{ item.REFNO }}">{{ item.EPEEL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SSOLID'><div>Soluble solids (%):</div><div><input type="number" step="0.01" min="0" placeholder="SSOLID" /></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='ISN'><div>ISN:</div><div><input type="number" step="1" min="0" placeholder="ISN" /></div></div>
            <div data-type='PH'><div>PH:</div><div><input type="number" step="0.01" min="0" placeholder="PH" /></div></div>
            <div data-type='BRIX'><div>BRIX:</div><div><input type="number" step="0.01" min="0" placeholder="BRIX" /></div></div>
            <div data-type='ACID'><div>ACID:</div><div><input type="number" step="0.01" min="0" placeholder="ACID" /></div></div>
            <div data-type='TSOLID'><div>TSOLID:</div><div><input type="number" step="0.01" min="0" placeholder="TSOLID" /></div></div>
            <div data-type='COLOR'><div>COLOR:</div><div><input type="number" step="0.01" min="0" placeholder="COLOR" /></div></div>
            <div data-type='CAROTEN'><div>CAROTEN:</div><div><input type="number" step="0.01" min="0" placeholder="CAROTEN" /></div></div>
            <div data-type='REMARKS'><div>Remarks:</div><div><input type="text" placeholder="REMARKS" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->