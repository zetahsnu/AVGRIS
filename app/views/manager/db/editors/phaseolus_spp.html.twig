<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>            
            <li><a href="#section-line-1"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-2"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-3"><span>FRUIT DATA</span></a></li>
            <li><a href="#section-line-4"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-5"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='HYPCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENPRILF'><div>Length of primary leaf (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENPRILF" /></div></div>
            <div data-type='WIDPRILF'><div>Width of primary leaf (cm):</div><div><input type="number" step="0.01" min="0" placeholder="WIDPRILF" /></div></div>
            <div data-type='GRHBT'><div>Growth habit:</div><div>
                    <select>
                        <option value="">* GRHBT *</option>
                        {% for item in related.GRHBT %}
                            <option value="{{ item.REFNO }}">{{ item.GRHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STCOL'><div>Stem color:</div><div>
                    <select>
                        <option value="">* STCOL *</option>
                        {% for item in related.STCOL %}
                            <option value="{{ item.REFNO }}">{{ item.STCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFPUB'><div>Leaf pubescence:</div><div>
                    <select>
                        <option value="">* LFPUB *</option>
                        {% for item in related.LFPUB %}
                            <option value="{{ item.REFNO }}">{{ item.LFPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENTLFL'><div>Length of tip leaflet (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENTLFL" /></div></div>
            <div data-type='WIDMILFL'><div>Width of mid leaflet (cm):</div><div><input type="number" step="0.01" min="0" placeholder="WIDMILFL" /></div></div>
            <div data-type='LFLSHP'><div>Leaflet shape:</div><div>
                    <select>
                        <option value="">* LFLSHP *</option>
                        {% for item in related.LFLSHP %}
                            <option value="{{ item.REFNO }}">{{ item.LFLSHP }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='DAYSFLO'><div>Days to flowering:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='FLOCOL'><div>Flower color:</div><div>
                    <select>
                        <option value="">* FLOCOL *</option>
                        {% for item in related.FLOCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLOCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PEDLEN'><div>Pedicel length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="PEDLEN" /></div></div>
            <div data-type='PEDULEN'><div>Peduncle length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="PEDULEN" /></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFMAT'><div>Days to first mature pod:</div><div><input type="number" step="1" min="0" placeholder="DAYSFMAT" /></div></div>
            <div data-type='IMATPPIG'><div>Pigment of immature pod pattern:</div><div>
                    <select>
                        <option value="">* IMATPPIG *</option>
                        {% for item in related.IMATPPIG %}
                            <option value="{{ item.REFNO }}">{{ item.IMATPPIG }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PODLEN'><div>Pod length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PODLEN" /></div></div>
            <div data-type='NOLOCU'><div>Number of locules per pod:</div><div><input type="number" step="0.01" min="0" placeholder="NOLOCU" /></div></div>
            <div data-type='PODCURV'><div>Pod curvature:</div><div>
                    <select>
                        <option value="">* PODCURV *</option>
                        {% for item in related.PODCURV %}
                            <option value="{{ item.REFNO }}">{{ item.PODCURV }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='SDCOL'><div>Seed color:</div><div>
                    <select>
                        <option value="">* SDCOL *</option>
                        {% for item in related.SDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SDSHP *</option>
                        {% for item in related.SDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.SDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TESTEX'><div>Testa texture:</div><div>
                    <select>
                        <option value="">* TESTEX *</option>
                        {% for item in related.TESTEX %}
                            <option value="{{ item.REFNO }}">{{ item.TESTEX }}</option>
                        {% endfor %}
                    </select>
                </div></div>
            <div data-type='HILCOL'><div>Hilum color:</div><div>
                    <select>
                        <option value="">* HILCOL *</option>
                        {% for item in related.HILCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HILCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
            <div data-type='SDLEN'><div>Seed length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDLEN" /></div></div>
            <div data-type='SDWID'><div>Seed width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWID" /></div></div>
            <div data-type='SDTHIC'><div>Seed thickness (mm):</div><div><input type="number" step="0.01" min="0" placeholder="SDTHIC" /></div></div>
            <div data-type='SDNOMP'><div>Seed number per mature pod:</div><div><input type="number" step="0.01" min="0" placeholder="SDNOMP" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-5">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->