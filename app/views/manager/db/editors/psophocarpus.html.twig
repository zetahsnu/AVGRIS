<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>            
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>POD DATA</span></a></li>
            <li><a href="#section-line-5"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='DAYSEMER'><div>Days to emergence:</div><div><input type="number" step="1" min="0" placeholder="DAYSEMER" /></div></div>
            <div data-type='HYPCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HYPCOLIN'><div>Hypocotyl color intensity:</div><div>
                    <select>
                        <option value="">* HYPCOLIN *</option>
                        {% for item in related.HYPCOLIN %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOLIN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HYPPUB'><div>Hypocotyl pubescence:</div><div>
                    <select>
                        <option value="">* HYPPUB *</option>
                        {% for item in related.HYPPUB %}
                            <option value="{{ item.REFNO }}">{{ item.HYPPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HYPLEN'><div>Hypocotyl length (in cm; from hypocotyl base to fully expanded primary leaves):</div><div><input type="number" step="0.01" min="0" placeholder="HYPLEN" /></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='PLGRO'><div>Plant growth (amount of plant growth at flowering):</div><div>
                    <select>
                        <option value="">* PLGRO *</option>
                        {% for item in related.PLGRO %}
                            <option value="{{ item.REFNO }}">{{ item.PLGRO }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFSZE'><div>Leaf size:</div><div>
                    <select>
                        <option value="">* LFSZE *</option>
                        {% for item in related.LFSZE %}
                            <option value="{{ item.REFNO }}">{{ item.LFSZE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFLSHP'><div>Leaflet shape:</div><div>
                    <select>
                        <option value="">* LFLSHP *</option>
                        {% for item in related.LFLSHP %}
                            <option value="{{ item.REFNO }}">{{ item.LFLSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STCOL'><div>Stem color:</div><div>
                    <select>
                        <option value="">* STCOL *</option>
                        {% for item in related.STCOL %}
                            <option value="{{ item.REFNO }}">{{ item.STCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PRETUB'><div>Presence of tuber:</div><div>
                    <select>
                        <option value="">* PRETUB *</option>
                        {% for item in related.PRETUB %}
                            <option value="{{ item.REFNO }}">{{ item.PRETUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TUBSZE'><div>Tuber size (diameter of the cross section of the widest point):</div><div>
                    <select>
                        <option value="">* TUBSZE *</option>
                        {% for item in related.TUBSZE %}
                            <option value="{{ item.REFNO }}">{{ item.TUBSZE }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='CALCOL'><div>Calyx color:</div><div>
                    <select>
                        <option value="">* CALCOL *</option>
                        {% for item in related.CALCOL %}
                            <option value="{{ item.REFNO }}">{{ item.CALCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='CORCOL'><div>Corolla color of wings and standard:</div><div>
                    <select>
                        <option value="">* CORCOL *</option>
                        {% for item in related.CORCOL %}
                            <option value="{{ item.REFNO }}">{{ item.CORCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='PODCOL'><div>Pod color (color of the central portion of the pod):</div><div>
                    <select>
                        <option value="">* PODCOL *</option>
                        {% for item in related.PODCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PODCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PREPODSP'><div>Presence of pod specks (purple specks on the central portion of the pod):</div><div>
                    <select>
                        <option value="">* PREPODSP *</option>
                        {% for item in related.PREPODSP %}
                            <option value="{{ item.REFNO }}">{{ item.PREPODSP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PODWICOL'><div>Pod wing color:</div><div>
                    <select>
                        <option value="">* PODWICOL *</option>
                        {% for item in related.PODWICOL %}
                            <option value="{{ item.REFNO }}">{{ item.PODWICOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PODSUTEX'><div>Pod surface texture:</div><div>
                    <select>
                        <option value="">* PODSUTEX *</option>
                        {% for item in related.PODSUTEX %}
                            <option value="{{ item.REFNO }}">{{ item.PODSUTEX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PODSHP'><div>Pod shape (shape in cross section at the center of the pods):</div><div>
                    <select>
                        <option value="">* PODSHP *</option>
                        {% for item in related.PODSHP %}
                            <option value="{{ item.REFNO }}">{{ item.PODSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PODLEN'><div>Pod length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PODLEN" /></div></div>
            <div data-type='PODSHAT'><div>Pod shattering:</div><div>
                    <select>
                        <option value="">* PODSHAT *</option>
                        {% for item in related.PODSHAT %}
                            <option value="{{ item.REFNO }}">{{ item.PODSHAT }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='NOSDSPOD'><div>Number of seeds per pod:</div><div><input type="number" step="0.01" min="0" placeholder="NOSDSPOD" /></div></div>
            <div data-type='SDCOL'><div>Seed color (when dried at harvest):</div><div>
                    <select>
                        <option value="">* SDCOL *</option>
                        {% for item in related.SDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PRESDMOT'><div>Presence of seed mottling:</div><div>
                    <select>
                        <option value="">* PRESDMOT *</option>
                        {% for item in related.PRESDMOT %}
                            <option value="{{ item.REFNO }}">{{ item.PRESDMOT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='HILCOL'><div>Hilum color:</div><div>
                    <select>
                        <option value="">* HILCOL *</option>
                        {% for item in related.HILCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HILCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SDSHP *</option>
                        {% for item in related.SDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.SDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>100 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->