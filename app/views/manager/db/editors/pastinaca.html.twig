<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>ROOT DATA</span></a></li>
            <li><a href="#section-line-5"><span>SEED DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERMPD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERMPD" /></div></div>
            <div data-type='HYPCOL'><div>Hypocotyl color:</div><div>
                    <select>
                        <option value="">* HYPCOL *</option>
                        {% for item in related.HYPCOL %}
                            <option value="{{ item.REFNO }}">{{ item.HYPCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COTLFCOL'><div>Cotyledon leaf color:</div><div>
                    <select>
                        <option value="">* COTLFCOL *</option>
                        {% for item in related.COTLFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.COTLFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='COTLFLEN'><div>Cotyledonous leaf length (mm):</div><div><input type="number" step="0.01" min="0" placeholder="COTLFLEN" /></div></div>
            <div data-type='COTLFWID'><div>Cotyledonous leaf width (mm):</div><div><input type="number" step="0.01" min="0" placeholder="COTLFWID" /></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='PLGRHBT'><div>Plant growth habit:</div><div>
                    <select>
                        <option value="">* PLGRHBT *</option>
                        {% for item in related.PLGRHBT %}
                            <option value="{{ item.REFNO }}">{{ item.PLGRHBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLDIA'><div>Plant diameter (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLDIA" /></div></div>
            <div data-type='NOLFPPL'><div>Number of leaves per plant:</div><div><input type="number" step="0.01" min="0" placeholder="NOLFPPL" /></div></div>
            <div data-type='LFCOL'><div>Leaf color:</div><div>
                    <select>
                        <option value="">* LFCOL *</option>
                        {% for item in related.LFCOL %}
                            <option value="{{ item.REFNO }}">{{ item.LFCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFSHP'><div>Leaf shape:</div><div>
                    <select>
                        <option value="">* LFSHP *</option>
                        {% for item in related.LFSHP %}
                            <option value="{{ item.REFNO }}">{{ item.LFSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFBLLOB'><div>Leaf blade lobbing:</div><div>
                    <select>
                        <option value="">* LFBLLOB *</option>
                        {% for item in related.LFBLLOB %}
                            <option value="{{ item.REFNO }}">{{ item.LFBLLOB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PETAROM'><div>Petiole aromatic:</div><div>
                    <select>
                        <option value="">* PETAROM *</option>
                        {% for item in related.PETAROM %}
                            <option value="{{ item.REFNO }}">{{ item.PETAROM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PETTHIC'><div>Petiole thickness (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PETTHIC" /></div></div>
            <div data-type='PETCOL'><div>Petiole color:</div><div>
                    <select>
                        <option value="">* PETCOL *</option>
                        {% for item in related.PETCOL %}
                            <option value="{{ item.REFNO }}">{{ item.PETCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='CHARPET'><div>Characteristic of petiole:</div><div>
                    <select>
                        <option value="">* CHARPET *</option>
                        {% for item in related.CHARPET %}
                            <option value="{{ item.REFNO }}">{{ item.CHARPET }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFLO'><div>Days to Flowering:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLO" /></div></div>
            <div data-type='FLCOL'><div>Flower color:</div><div>
                    <select>
                        <option value="">* FLCOL *</option>
                        {% for item in related.FLCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='ROOTSHP'><div>Root shape:</div><div>
                    <select>
                        <option value="">* ROOTSHP *</option>
                        {% for item in related.ROOTSHP %}
                            <option value="{{ item.REFNO }}">{{ item.ROOTSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='ROOTLEN'><div>Root length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="ROOTLEN" /></div></div>
            <div data-type='ROOTDIA'><div>Root diameter (cm):</div><div><input type="number" step="0.01" min="0" placeholder="ROOTDIA" /></div></div>
            <div data-type='ROOTWT'><div>Root weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="ROOTWT" /></div></div>
            <div data-type='ROOTSHPSH'><div>Root shape of shoulder:</div><div>
                    <select>
                        <option value="">* ROOTSHPSH *</option>
                        {% for item in related.ROOTSHPSH %}
                            <option value="{{ item.REFNO }}">{{ item.ROOTSHPSH }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='ROOTSHPBS'><div>Root shape at base:</div><div>
                    <select>
                        <option value="">* ROOTSHPBS *</option>
                        {% for item in related.ROOTSHPBS %}
                            <option value="{{ item.REFNO }}">{{ item.ROOTSHPBS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='EXROOTCOL'><div>Exterior root color:</div><div>
                    <select>
                        <option value="">* EXROOTCOL *</option>
                        {% for item in related.EXROOTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.EXROOTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='ROOTEXCOLP'><div>Root exterior color pattern:</div><div>
                    <select>
                        <option value="">* ROOTEXCOLP *</option>
                        {% for item in related.ROOTEXCOLP %}
                            <option value="{{ item.REFNO }}">{{ item.ROOTEXCOLP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='INROOTCOL'><div>Interior root color:</div><div>
                    <select>
                        <option value="">* INROOTCOL *</option>
                        {% for item in related.INROOTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.INROOTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLESHCOL'><div>Flesh color in transverse section:</div><div>
                    <select>
                        <option value="">* FLESHCOL *</option>
                        {% for item in related.FLESHCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLESHCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='SDSHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SDSHP *</option>
                        {% for item in related.SDSHP %}
                            <option value="{{ item.REFNO }}">{{ item.SDSHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDCOL'><div>Seed color:</div><div>
                    <select>
                        <option value="">* SDCOL *</option>
                        {% for item in related.SDCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SDCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SDWT'><div>1000 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SDWT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->