<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input id="ACCNO" type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input id="SPECIES" type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input id="DUPLIC" type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input id="SUBTAXA" type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input id="TEMPNO" type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>SEEDLING DATA</span></a></li>
            <li><a href="#section-line-2"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-3"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-4"><span>SEED DATA</span></a></li>
            <li><a href="#section-line-5"><span>Other</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-6"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='GERM_PD'><div>Germination period:</div><div><input type="number" step="1" min="0" placeholder="GERM_PD" /></div></div>
            <div data-type='GERMRATE'><div>Germination rate:</div><div>
                    <select>
                        <option value="">* GERMRATE *</option>
                        {% for item in related.GERMRATE %}
                            <option value="{{ item.REFNO }}">{{ item.GERMRATE }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='G_HBT'><div>Growth habit:</div><div>
                    <select>
                        <option value="">* G_HBT *</option>
                        {% for item in related.G_HBT %}
                            <option value="{{ item.REFNO }}">{{ item.G_HBT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PLHEIGHT'><div>Plant height (cm):</div><div><input type="number" step="0.01" min="0" placeholder="PLHEIGHT" /></div></div>
            <div data-type='BR_INDX'><div>Branching index:</div><div>
                    <select>
                        <option value="">* BR_INDX *</option>
                        {% for item in related.BR_INDX %}
                            <option value="{{ item.REFNO }}">{{ item.BR_INDX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='MLENGBAS'><div>Mean length of basal lateral branches (cm):</div><div><input type="number" step="0.01" min="0" placeholder="MLENGBAS" /></div></div>
            <div data-type='MLENGTLB'><div>Mean length of top lateral branches (cm):</div><div><input type="number" step="0.01" min="0" placeholder="MLENGTLB" /></div></div>
            <div data-type='STEMPUB'><div>Stem pubescence:</div><div>
                    <select>
                        <option value="">* STEMPUB *</option>
                        {% for item in related.STEMPUB %}
                            <option value="{{ item.REFNO }}">{{ item.STEMPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='STEMPIG'><div>Stem pigmentation:</div><div>
                    <select>
                        <option value="">* STEMPIG *</option>
                        {% for item in related.STEMPIG %}
                            <option value="{{ item.REFNO }}">{{ item.STEMPIG }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SPINELAX'><div>Spines in leaf axils:</div><div>
                    <select>
                        <option value="">* SPINELAX *</option>
                        {% for item in related.SPINELAX %}
                            <option value="{{ item.REFNO }}">{{ item.SPINELAX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LEAFLEN'><div>Leaf length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LEAFLEN" /></div></div>
            <div data-type='LEAFWID'><div>Leaf width (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LEAFWID" /></div></div>
            <div data-type='LEAFPUB'><div>Leaf pubescence:</div><div>
                    <select>
                        <option value="">* LEAFPUB *</option>
                        {% for item in related.LEAFPUB %}
                            <option value="{{ item.REFNO }}">{{ item.LEAFPUB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LEAFPIG'><div>Leaf pigmentation:</div><div>
                    <select>
                        <option value="">* LEAFPIG *</option>
                        {% for item in related.LEAFPIG %}
                            <option value="{{ item.REFNO }}">{{ item.LEAFPIG }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LEAFSHPE'><div>Leaf shape:</div><div>
                    <select>
                        <option value="">* LEAFSHPE *</option>
                        {% for item in related.LEAFSHPE %}
                            <option value="{{ item.REFNO }}">{{ item.LEAFSHPE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LEAFMARGN'><div>Leaf margin:</div><div>
                    <select>
                        <option value="">* LEAFMARGN *</option>
                        {% for item in related.LEAFMARGN %}
                            <option value="{{ item.REFNO }}">{{ item.LEAFMARGN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PROLEAFVEIN'><div>Prominence of leaf veins:</div><div>
                    <select>
                        <option value="">* PROLEAFVEIN *</option>
                        {% for item in related.PROLEAFVEIN %}
                            <option value="{{ item.REFNO }}">{{ item.PROLEAFVEIN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PETPIG'><div>Petiole pigmentation:</div><div>
                    <select>
                        <option value="">* PETPIG *</option>
                        {% for item in related.PETPIG %}
                            <option value="{{ item.REFNO }}">{{ item.PETPIG }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='DAYSFLOW'><div>Days to flowering:</div><div><input type="number" step="1" min="0" placeholder="DAYSFLOW" /></div></div>
            <div data-type='TERM_INFLO'><div>Terminal inflorescence laterals length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="TERM_INFLO" /></div></div>
            <div data-type='TERM_INFSTK'><div>Terminal inflorescence stalk length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="TERM_INFSTK" /></div></div>
            <div data-type='TERM_INF_SHAPE'><div>Terminal inflorescence shape:</div><div>
                    <select>
                        <option value="">* TERM_INF_SHAPE *</option>
                        {% for item in related.TERM_INF_SHAPE %}
                            <option value="{{ item.REFNO }}">{{ item.TERM_INF_SHAPE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='TERM_ATTI'><div>Terminal inflorescence attitude:</div><div>
                    <select>
                        <option value="">* TERM_ATTI *</option>
                        {% for item in related.TERM_ATTI %}
                            <option value="{{ item.REFNO }}">{{ item.TERM_ATTI }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PRESAXI'><div>Presence of axillary inflorescence:</div><div>
                    <select>
                        <option value="">* PRESAXI *</option>
                        {% for item in related.PRESAXI %}
                            <option value="{{ item.REFNO }}">{{ item.PRESAXI }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LENG_AXI'><div>Length of axillary inflorescence (cm):</div><div><input type="number" step="0.01" min="0" placeholder="LENG_AXI" /></div></div>
            <div data-type='STYPE'><div>Sex type:</div><div>
                    <select>
                        <option value="">* STYPE *</option>
                        {% for item in related.STYPE %}
                            <option value="{{ item.REFNO }}">{{ item.STYPE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='INFLODEN'><div>Inflorescence density index:</div><div>
                    <select>
                        <option value="">* INFLODEN *</option>
                        {% for item in related.INFLODEN %}
                            <option value="{{ item.REFNO }}">{{ item.INFLODEN }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='INFLOCOL'><div>Inflorescence color:</div><div>
                    <select>
                        <option value="">* INFLOCOL *</option>
                        {% for item in related.INFLOCOL %}
                            <option value="{{ item.REFNO }}">{{ item.INFLOCOL }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-4">
            <div data-type='SD_SHAT'><div>Seed shattering:</div><div>
                    <select>
                        <option value="">* SD_SHAT *</option>
                        {% for item in related.SD_SHAT %}
                            <option value="{{ item.REFNO }}">{{ item.SD_SHAT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SD_COLOR'><div>Seed color:</div><div>
                    <select>
                        <option value="">* SD_COLOR *</option>
                        {% for item in related.SD_COLOR %}
                            <option value="{{ item.REFNO }}">{{ item.SD_COLOR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SD_CTYPE'><div>Seed coat type:</div><div>
                    <select>
                        <option value="">* SD_CTYPE *</option>
                        {% for item in related.SD_CTYPE %}
                            <option value="{{ item.REFNO }}">{{ item.SD_CTYPE }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SD_SHP'><div>Seed shape:</div><div>
                    <select>
                        <option value="">* SD_SHP *</option>
                        {% for item in related.SD_SHP %}
                            <option value="{{ item.REFNO }}">{{ item.SD_SHP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SD_WT'><div>1000 seeds weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="SD_WT" /></div></div>
        </section>
        <section id="section-line-5">
            <div data-type='REMARKS'><div>Remarks:</div><div><input type="text" placeholder="REMARKS" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-6">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->