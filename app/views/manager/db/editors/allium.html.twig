<div id='editorMain'>
    <div data-type='VINO'><div>Vegetable introduction number:</div><div><input id="disVINO" type="text" placeholder="VINO" disabled /><input id="inputVINO" type="text"><button id="searchVINO" class="button" data-status="search" title="Search VINO From Passport"></button></div></div>
    <div data-type='VARIANT'><div>Variant:</div><div><input type="text" placeholder="VARIANT" /></div></div>
    <div data-type='ACCNO'><div>Crop accession number:</div><div><input type="text" placeholder="ACCNO" disabled /></div></div>
    <div data-type='SPECIES'><div>Species:</div><div><input type="text" placeholder="SPECIES" disabled /></div></div>
    <div data-type='DUPLIC'><div>Duplicate note:</div><div><input type="text" placeholder="DUPLIC" disabled /></div></div>
    <div data-type='SUBTAXA'><div>Subtaxa:</div><div><input type="text" placeholder="SUBTAXA" disabled /></div></div>
    <div data-type='TEMPNO'><div>Temporary number:</div><div><input type="text" placeholder="TEMPNO" disabled /></div></div>
    <div data-type='YRSN'><div>Char. year & season:</div><div><input type="text" placeholder="YRSN" /></div></div>
</div>
<div id='editorOther' class="tabs tabs-style-line csh4">
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>VEGETATIVE DATA</span></a></li>
            <li><a href="#section-line-2"><span>INFLORESCENCE DATA</span></a></li>
            <li><a href="#section-line-3"><span>BULB DATA</span></a></li>
                {% if SESSION.user.JURISDICTION == 0 %}
                <li><a href="#section-line-4"><span>Photo</span></a></li>
                {% endif %}
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div data-type='FOLCOL'><div>Foliage color:</div><div>
                    <select>
                        <option value="">* FOLCOL *</option>
                        {% for item in related.FOLCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FOLCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LFLEN'><div>Leaf length (cm):</div><div><input type="number" step="1" min="0" placeholder="LFLEN" /></div></div>
            <div data-type='LFERECT'><div>Leaf erectness:</div><div>
                    <select>
                        <option value="">* LFERECT *</option>
                        {% for item in related.LFERECT %}
                            <option value="{{ item.REFNO }}">{{ item.LFERECT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='CRSSLF'><div>Cross section of leaf:</div><div>
                    <select>
                        <option value="">* CRSSLF *</option>
                        {% for item in related.CRSSLF %}
                            <option value="{{ item.REFNO }}">{{ item.CRSSLF }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PRLFWX'><div>Present of leaf wax:</div><div>
                    <select>
                        <option value="">* PRLFWX *</option>
                        {% for item in related.PRLFWX %}
                            <option value="{{ item.REFNO }}">{{ item.PRLFWX }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='LIFESP'><div>Life span:</div><div>
                    <select>
                        <option value="">* LIFESP *</option>
                        {% for item in related.LIFESP %}
                            <option value="{{ item.REFNO }}">{{ item.LIFESP }}</option>
                        {% endfor %}
                    </select></div></div>
        </section>
        <section id="section-line-2">
            <div data-type='FL_ABLTY'><div>Ability to flower:</div><div>
                    <select>
                        <option value="">* FL_ABLTY *</option>
                        {% for item in related.FL_ABLTY %}
                            <option value="{{ item.REFNO }}">{{ item.FL_ABLTY }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='GENFER'><div>General fertility:</div><div>
                    <select>
                        <option value="">* GENFER *</option>
                        {% for item in related.GENFER %}
                            <option value="{{ item.REFNO }}">{{ item.GENFER }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLNO_UM'><div>Flower number in umbel:</div><div>
                    <select>
                        <option value="">* FLNO_UM *</option>
                        {% for item in related.FLNO_UM %}
                            <option value="{{ item.REFNO }}">{{ item.FLNO_UM }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLCOL'><div>Flower color:</div><div>
                    <select>
                        <option value="">* FLCOL *</option>
                        {% for item in related.FLCOL %}
                            <option value="{{ item.REFNO }}">{{ item.FLCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='ANTCOL'><div>Anther color:</div><div>
                    <select>
                        <option value="">* ANTCOL *</option>
                        {% for item in related.ANTCOL %}
                            <option value="{{ item.REFNO }}">{{ item.ANTCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='MODREP'><div>Mode of reproduction:</div><div>
                    <select>
                        <option value="">* MODREP *</option>
                        {% for item in related.MODREP %}
                            <option value="{{ item.REFNO }}">{{ item.MODREP }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SCCOL'><div>Seed coat color:</div><div>
                    <select>
                        <option value="">* SCCOL *</option>
                        {% for item in related.SCCOL %}
                            <option value="{{ item.REFNO }}">{{ item.SCCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='SCLEN'><div>Scape length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="SCLEN" /></div></div>
            <div data-type='INTSTRC'><div>Internal structure of mature scape:</div><div>
                    <select>
                        <option value="">* INTSTRC *</option>
                        {% for item in related.INTSTRC %}
                            <option value="{{ item.REFNO }}">{{ item.INTSTRC }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='FLWDATE'><div>Flowering date:</div><div><input type="date" placeholder="FLWDATE" /></div></div>
            <div data-type='MATDATE'><div>Date of maturity:</div><div><input type="date" placeholder="MATDATE" /></div></div>
        </section>
        <section id="section-line-3">
            <div data-type='SHP_FGB'><div>Shape of full-grown bulbs:</div><div>
                    <select>
                        <option value="">* SHP_FGB *</option>
                        {% for item in related.SHP_FGB %}
                            <option value="{{ item.REFNO }}">{{ item.SHP_FGB }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BLB_SKCOL'><div>Bulb skin color:</div><div>
                    <select>
                        <option value="">* BLB_SKCOL *</option>
                        {% for item in related.BLB_SKCOL %}
                            <option value="{{ item.REFNO }}">{{ item.BLB_SKCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PR_BULBTS'><div>Presence of bulblets:</div><div>
                    <select>
                        <option value="">* PR_BULBTS *</option>
                        {% for item in related.PR_BULBTS %}
                            <option value="{{ item.REFNO }}">{{ item.PR_BULBTS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='PR_BULBLS'><div>Presence of bulbils:</div><div>
                    <select>
                        <option value="">* PR_BULBLS *</option>
                        {% for item in related.PR_BULBLS %}
                            <option value="{{ item.REFNO }}">{{ item.PR_BULBLS }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='NAT_STOR'><div>Nature of storage organ:</div><div>
                    <select>
                        <option value="">* NAT_STOR *</option>
                        {% for item in related.NAT_STOR %}
                            <option value="{{ item.REFNO }}">{{ item.NAT_STOR }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BLB_STHCK'><div>Bulb skin thickness:</div><div>
                    <select>
                        <option value="">* BLB_STHCK *</option>
                        {% for item in related.BLB_STHCK %}
                            <option value="{{ item.REFNO }}">{{ item.BLB_STHCK }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BLB_FRCOL'><div>Bulb fresh color:</div><div>
                    <select>
                        <option value="">* BLB_FRCOL *</option>
                        {% for item in related.BLB_FRCOL %}
                            <option value="{{ item.REFNO }}">{{ item.BLB_FRCOL }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BLB_HRT'><div>Bulb heart:</div><div>
                    <select>
                        <option value="">* BLB_HRT *</option>
                        {% for item in related.BLB_HRT %}
                            <option value="{{ item.REFNO }}">{{ item.BLB_HRT }}</option>
                        {% endfor %}
                    </select></div></div>
            <div data-type='BLB_LEN'><div>Bulb length (cm):</div><div><input type="number" step="0.01" min="0" placeholder="BLB_LEN" /></div></div>
            <div data-type='BLB_DIA'><div>Bulb diameter (cm):</div><div><input type="number" step="0.01" min="0" placeholder="BLB_DIA" /></div></div>
            <div data-type='BLB_WGT'><div>Bulb weight (gm):</div><div><input type="number" step="0.01" min="0" placeholder="BLB_WGT" /></div></div>
        </section>
        {% if SESSION.user.JURISDICTION == 0 %}
            <section id="section-line-4">
                {% embed "/manager/db/editors/picture/picture.html.twig" %}
                {% endembed %}
            </section>
        {% endif %}
    </div><!-- /content -->
</div><!-- /tabs -->